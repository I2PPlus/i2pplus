msgid "Failed to load the wrapper"
msgstr "Échec de chargement de l’enveloppeur"

#, sh-format
msgid "$APP_LONG_NAME is already running."
msgstr "$APP_LONG_NAME est déjà en cours d’exécution."

#, sh-format
msgid "Running $APP_LONG_NAME"
msgstr "Exécution de $APP_LONG_NAME"

#, sh-format
msgid "Waiting for $APP_LONG_NAME"
msgstr "En attente de $APP_LONG_NAME"

#, sh-format
msgid "WARNING: $APP_LONG_NAME may have failed to start."
msgstr "AVERTISSEMENT : Échec possible de démarrage de $APP_LONG_NAME."

msgid "Must be root to perform this action."
msgstr "Vous devez être root pour effectuer cette action."

#, sh-format
msgid "Starting $APP_LONG_NAME"
msgstr "Démarrage de $APP_LONG_NAME"

#, sh-format
msgid "Stopping $APP_LONG_NAME"
msgstr "Arrêt de $APP_LONG_NAME"

#, sh-format
msgid "$APP_LONG_NAME was not running."
msgstr "$APP_LONG_NAME n’était pas en cours d’exécution."

#, sh-format
msgid "Unable to stop $APP_LONG_NAME."
msgstr "Impossible de stopper $APP_LONG_NAME."

#, sh-format
msgid "Waiting for $APP_LONG_NAME to exit"
msgstr "En attente de fermeture de $APP_LONG_NAME."

#, sh-format
msgid "Failed to stop $APP_LONG_NAME."
msgstr "Échec d’arrêt de $APP_LONG_NAME."

#, sh-format
msgid "Stopped $APP_LONG_NAME."
msgstr "$APP_LONG_NAME a été arrêtée."

#, sh-format
msgid "Stopping $APP_LONG_NAME gracefully"
msgstr "Fermeture respectueuse de $APP_LONG_NAME"

#, sh-format
msgid "$APP_LONG_NAME is not running."
msgstr "$APP_LONG_NAME n’est pas en cours d’exécution."

#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid"
msgstr "$APP_LONG_NAME est en cours d’exécution : PID:$pid"

#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid, Wrapper:$STATUS, Java:$JAVASTATUS"
msgstr "$APP_LONG_NAME est en cours d’exécution: PID:$pid, Enveloppeur:$STATUS, Java:$JAVASTATUS"

#, sh-format
msgid "The $APP_LONG_NAME daemon is already installed."
msgstr "Le démon de $APP_LONG_NAME est déjà installé."

#, sh-format
msgid "Installing the $APP_LONG_NAME daemon"
msgstr "Installation du démon de $APP_LONG_NAME"

#, sh-format
msgid "Install not currently supported for $DIST_OS"
msgstr "L’installation n’est actuellement pas prise en charge pour $DIST_OS"

#, sh-format
msgid "Removing $APP_LONG_NAME daemon"
msgstr "Suppression du démon de $APP_LONG_NAME"

#, sh-format
msgid "The $APP_LONG_NAME daemon is not currently installed."
msgstr "Le démon de $APP_LONG_NAME n’est actuellement pas installé."

#, sh-format
msgid "Remove not currently supported for $DIST_OS"
msgstr "La suppression n’est actuellement pas prise en charge pour $DIST_OS"

msgid "Commands:"
msgstr "Commandes :"

msgid "Launch in the current console."
msgstr "Lancer dans la console actuelle."

msgid "Start in the background as a daemon process."
msgstr "Démarrer en arrière-plan comme processus démon."

msgid "Stop if running as a daemon or in another console."
msgstr "Arrêter si elle fonctionne comme démon ou dans une autre console."

msgid "Stop gracefully, may take up to 11 minutes or more."
msgstr "Arrêt respectueux, peut prendre jusqu’à 11 minutes."

msgid "Restart gracefully, may take up to 11 minutes or more."
msgstr "Redémarrez normalement, cela peut prendre jusqu'à 11 minutes."

msgid "Stop if running and then start."
msgstr "Arrêter si elle fonctionne et démarrer ensuite."

msgid "Restart only if already running."
msgstr "Redémarrer seulement si elle est déjà en cours d’exécution."

msgid "Query the current status."
msgstr "Demander l’état actuel."

msgid "Install to start automatically when system boots."
msgstr "Installer pour lancer automatiquement quand le système système démarre."

msgid "Uninstall."
msgstr "Désinstaller."

msgid "Request a Java thread dump if running."
msgstr "Demander un vidage des fils d’exécution de Java si elle est en cours d’exécution."

msgid "Please edit i2prouter and set the variable RUN_AS_USER"
msgstr "Veuillez modifier i2prouter et définir la variable RUN_AS_USER"

msgid "Running I2P as the root user is *not* recommended."
msgstr "Il n’est *pas* recommandé d’exécuter I2P en tant que root."

msgid "To run as root anyway, edit i2prouter and set ALLOW_ROOT=true."
msgstr "Pour quand même exécuter en tant que root, modifier i2prouter et définir ALLOW_ROOT=true."
