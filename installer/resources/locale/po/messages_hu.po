#: ../i2prouter:236
msgid "Failed to load the wrapper"
msgstr "A behúzó betöltése sikertelen volt"

#: ../i2prouter:1021 ../i2prouter:1049 ../i2prouter:1123 ../i2prouter:1151
#: ../i2prouter:1172
#, sh-format
msgid "$APP_LONG_NAME is already running."
msgstr "$APP_LONG_NAME már fut."

#: ../i2prouter:1032
#, sh-format
msgid "Running $APP_LONG_NAME"
msgstr "$APP_LONG_NAME futtatása"

#: ../i2prouter:1056
#, sh-format
msgid "Waiting for $APP_LONG_NAME"
msgstr "$APP_LONG_NAME-re várakozás"

#: ../i2prouter:1103
#, sh-format
msgid "WARNING: $APP_LONG_NAME may have failed to start."
msgstr "FIGYELEM: $APP_LONG_NAME elindítása sikertelen lehetett."

#: ../i2prouter:1117 ../i2prouter:1145 ../i2prouter:1351 ../i2prouter:1640
msgid "Must be root to perform this action."
msgstr "Ez a művelet végrehajtásához rootnak kell lenni."

#: ../i2prouter:1161
#, sh-format
msgid "Starting $APP_LONG_NAME"
msgstr "$APP_LONG_NAME indítása"

#: ../i2prouter:1183
#, sh-format
msgid "Stopping $APP_LONG_NAME"
msgstr "$APP_LONG_NAME megállítása"

#: ../i2prouter:1187 ../i2prouter:1255 ../i2prouter:1808
#, sh-format
msgid "$APP_LONG_NAME was not running."
msgstr "$APP_LONG_NAME nem futott."

#: ../i2prouter:1200 ../i2prouter:1208 ../i2prouter:1270 ../i2prouter:1278
#, sh-format
msgid "Unable to stop $APP_LONG_NAME."
msgstr "$APP_LONG_NAME megállítása nem lehetséges."

#: ../i2prouter:1226
#, sh-format
msgid "Waiting for $APP_LONG_NAME to exit"
msgstr "$APP_LONG_NAME kilépésére várakozás"

#: ../i2prouter:1240
#, sh-format
msgid "Failed to stop $APP_LONG_NAME."
msgstr "$APP_LONG_NAME megállítása sikertelen."

#: ../i2prouter:1243
#, sh-format
msgid "Stopped $APP_LONG_NAME."
msgstr "$APP_LONG_NAME megállítva."

#: ../i2prouter:1251
#, sh-format
msgid "Stopping $APP_LONG_NAME gracefully"
msgstr "$APP_LONG_NAME elegáns megállítása"

#: ../i2prouter:1297
#, sh-format
msgid "$APP_LONG_NAME is not running."
msgstr "$APP_LONG_NAME nem fut."

#: ../i2prouter:1302
#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid"
msgstr "$APP_LONG_NAME fut: PID:$pid"

#: ../i2prouter:1305
#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid, Wrapper:$STATUS, Java:$JAVASTATUS"
msgstr "$APP_LONG_NAME fut: PID:$pid, Wrapper:$STATUS, Java:$JAVASTATUS"

#: ../i2prouter:1358 ../i2prouter:1370 ../i2prouter:1389 ../i2prouter:1406
#: ../i2prouter:1473 ../i2prouter:1493 ../i2prouter:1507 ../i2prouter:1521
#: ../i2prouter:1549 ../i2prouter:1587 ../i2prouter:1622
#, sh-format
msgid "The $APP_LONG_NAME daemon is already installed."
msgstr "$APP_LONG_NAME szolgáltatás már telepítve van."

#: ../i2prouter:1361 ../i2prouter:1376 ../i2prouter:1478 ../i2prouter:1510
#: ../i2prouter:1524 ../i2prouter:1538 ../i2prouter:1552 ../i2prouter:1590
#: ../i2prouter:1625
#, sh-format
msgid "Installing the $APP_LONG_NAME daemon"
msgstr "$APP_LONG_NAME szolgáltatás telepítése"

#: ../i2prouter:1632
#, sh-format
msgid "Install not currently supported for $DIST_OS"
msgstr "Telepítés nem támogatott jelenleg a $DIST_OS számára"

#: ../i2prouter:1648 ../i2prouter:1661 ../i2prouter:1675 ../i2prouter:1684
#: ../i2prouter:1694 ../i2prouter:1718 ../i2prouter:1731 ../i2prouter:1743
#: ../i2prouter:1761 ../i2prouter:1774 ../i2prouter:1788
#, sh-format
msgid "Removing $APP_LONG_NAME daemon"
msgstr "$APP_LONG_NAME szolgáltatás eltávolítása"

#: ../i2prouter:1654 ../i2prouter:1669 ../i2prouter:1678 ../i2prouter:1688
#: ../i2prouter:1699 ../i2prouter:1712 ../i2prouter:1724 ../i2prouter:1737
#: ../i2prouter:1755 ../i2prouter:1768 ../i2prouter:1782 ../i2prouter:1793
#, sh-format
msgid "The $APP_LONG_NAME daemon is not currently installed."
msgstr "$APP_LONG_NAME szolgáltatás jelenleg nem telepítve."

#: ../i2prouter:1797
#, sh-format
msgid "Remove not currently supported for $DIST_OS"
msgstr "Eltávolítás nem támogatott jelenleg a $DIST_OS számára"

#: ../i2prouter:1884
msgid "Commands:"
msgstr "Parancsok:"

#: ../i2prouter:1885
msgid "Launch in the current console."
msgstr "Indítás a jelenlegi konzolban."

#: ../i2prouter:1886
msgid "Start in the background as a daemon process."
msgstr "Szolgáltatásként háttérben indítás."

#: ../i2prouter:1887
msgid "Stop if running as a daemon or in another console."
msgstr "Megállítás, ha szolgáltatásként vagy egy másik konzolban fut."

#: ../i2prouter:1888
msgid "Stop gracefully, may take up to 11 minutes or more."
msgstr "Elegáns megállítás, ez 11 perc vagy több ideig is telhet."

#: ../i2prouter:1889
msgid "Restart gracefully, may take up to 11 minutes or more."
msgstr "Elegáns újraindítás, ez 11 perc vagy több ideig is telhet."

#: ../i2prouter:1890
msgid "Stop if running and then start."
msgstr "Megállítás, ha fut, majd indítás."

#: ../i2prouter:1891
msgid "Restart only if already running."
msgstr "Csak újraindítás, ha már fut."

#: ../i2prouter:1892
msgid "Query the current status."
msgstr "A jelenlegi állapot lekérdezése."

#: ../i2prouter:1893
msgid "Install to start automatically when system boots."
msgstr "Telepítés automatikus indításhoz a rendszer indulásakor."

#: ../i2prouter:1894
msgid "Uninstall."
msgstr "Telepítés visszavonása."

#: ../i2prouter:1895
msgid "Request a Java thread dump if running."
msgstr "Java folyamat dátumát kérje le, ha fut."

#: ../i2prouter:1908
msgid "Please edit i2prouter and set the variable RUN_AS_USER"
msgstr "Kérjük, szerkessze az i2prouter-t és állítsa a RUN_AS_USER változót"

#: ../i2prouter:1913
msgid "Running I2P as the root user is *not* recommended."
msgstr "Az I2P futtatása root felhasználóként *nem ajánlott*."

#: ../i2prouter:1916
msgid "To run as root anyway, edit i2prouter and set ALLOW_ROOT=true."
msgstr "Ha bármivel rootként szeretne futtatni, szerkessze az i2proutert, és állítsa a ALLOW_ROOT értékét igazra."