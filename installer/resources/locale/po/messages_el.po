msgid "Failed to load the wrapper"
msgstr "Αποτυχία φόρτωσης του εξωτερικού περιβάλλοντος"

#, sh-format
msgid "$APP_LONG_NAME is already running."
msgstr "$APP_LONG_NAME τρέχει ήδη."

#, sh-format
msgid "Running $APP_LONG_NAME"
msgstr "Τρέχει $APP_LONG_NAME"

#, sh-format
msgid "Waiting for $APP_LONG_NAME"
msgstr "Προσδοκώ $APP_LONG_NAME"

#, sh-format
msgid "WARNING: $APP_LONG_NAME may have failed to start."
msgstr "ΠΡΟΕΙΔΩΠΣΗ: Ο $APP_LONG_NAME μπορεί να έχει αποτύχει να ξεκινήσει."

msgid "Must be root to perform this action."
msgstr "Πρέπει να είστε root για να εκτελέσετε αυτή τη δράση."

#, sh-format
msgid "Starting $APP_LONG_NAME"
msgstr "Έναρξη $APP_LONG_NAME"

#, sh-format
msgid "Stopping $APP_LONG_NAME"
msgstr "Αναστολή $APP_LONG_NAME"

#, sh-format
msgid "$APP_LONG_NAME was not running."
msgstr "$APP_LONG_NAME δεν τρέχει."

#, sh-format
msgid "Unable to stop $APP_LONG_NAME."
msgstr "Δεν μπορώ να σταματήσω το $APP_LONG_NAME."

#, sh-format
msgid "Waiting for $APP_LONG_NAME to exit"
msgstr "Προσδοκώ το $APP_LONG_NAME να τερματίσει"

#, sh-format
msgid "Failed to stop $APP_LONG_NAME."
msgstr "Αποτυχία σταμάτημα $APP_LONG_NAME."

#, sh-format
msgid "Stopped $APP_LONG_NAME."
msgstr "Σταμάτησα $APP_LONG_NAME."

#, sh-format
msgid "Stopping $APP_LONG_NAME gracefully"
msgstr "Σταματάω ευγενικά $APP_LONG_NAME"

#, sh-format
msgid "$APP_LONG_NAME is not running."
msgstr "$APP_LONG_NAME δεν τρέχει."

#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid"
msgstr "$APP_LONG_NAME τρέχει: PID:$pid"

#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid, Wrapper:$STATUS, Java:$JAVASTATUS"
msgstr "$APP_LONG_NAME τρέχει: PID:$pid, Wrapper:$STATUS, Java:$JAVASTATUS"

#, sh-format
msgid "The $APP_LONG_NAME daemon is already installed."
msgstr "Ο δαίμων $APP_LONG_NAME είναι ήδη εγκατεστημένος."

#, sh-format
msgid "Installing the $APP_LONG_NAME daemon"
msgstr "Εγκατάσταση του δαίμονα $APP_LONG_NAME"

#, sh-format
msgid "Install not currently supported for $DIST_OS"
msgstr "Η εγκατάσταση δεν υποστηρίζεται αυτή τη στιγμή για $DIST_OS"

#, sh-format
msgid "Removing $APP_LONG_NAME daemon"
msgstr "Αφαίρεση του δαίμονα $APP_LONG_NAME"

#, sh-format
msgid "The $APP_LONG_NAME daemon is not currently installed."
msgstr "Ο δαίμων $APP_LONG_NAME δεν είναι εγκατεστημένος αυτή τη στιγμή."

#, sh-format
msgid "Remove not currently supported for $DIST_OS"
msgstr "Η αφαίρεση δεν υποστηρίζεται αυτή τη στιγμή για $DIST_OS"

msgid "Commands:"
msgstr "Εντολές:"

msgid "Launch in the current console."
msgstr "Εκτέλεση στην τρέχουσα κονσόλα."

msgid "Start in the background as a daemon process."
msgstr "Έναρξη στο παρασκευαστικό ως διαδικασία δαίμονα."

msgid "Stop if running as a daemon or in another console."
msgstr "Σταματήστε αν τρέχει ως δαίμων ή σε άλλη κονσόλα."

msgid "Stop gracefully, may take up to 11 minutes or more."
msgstr "Σταματήστε ευγενικά, μπορεί να πάρει έως και 11 λεπτά ή περισσότερο."

msgid "Restart gracefully, may take up to 11 minutes or more."
msgstr "Επανεκκίνηση ευγενικά, μπορεί να πάρει έως και 11 λεπτά ή περισσότερο."

msgid "Stop if running and then start."
msgstr "Σταματήστε αν τρέχει και στη συνέχεια ξεκινήστε."

msgid "Restart only if already running."
msgstr "Επανεκκίνηση μόνο αν τρέχει ήδη."

msgid "Query the current status."
msgstr "Ερώτηση της τρέχουσας κατάστασης."

msgid "Install to start automatically when system boots."
msgstr "Εγκατάσταση για αυτόματη έναρξη όταν ξεκινά το σύστημα."

msgid "Uninstall."
msgstr "Αφαίρεση."

msgid "Request a Java thread dump if running."
msgstr "Ζητήστε αποθήκευση νήματος Java αν τρέχει."

msgid "Please edit i2prouter and set the variable RUN_AS_USER"
msgstr "Παρακαλώ επεξεργαστείτε το i2prouter και ορίστε τη μεταβλητή RUN_AS_USER"

msgid "Running I2P as the root user is *not* recommended."
msgstr "Η εκτέλεση του I2P ως χρήστη root *δεν* προτείνεται."

msgid "To run as root anyway, edit i2prouter and set ALLOW_ROOT=true."
msgstr "Για να τρέξετε ως root οποιοσδήποτε, επεξεργαστείτε το i2prouter και ορίστε το ALLOW_ROOT=true."
