msgid "Failed to load the wrapper"
msgstr "Laden van wrapper mislukt"

#, sh-format
msgid "$APP_LONG_NAME is already running."
msgstr "$APP_LONG_NAME draait al."

#, sh-format
msgid "Running $APP_LONG_NAME"
msgstr "$APP_LONG_NAME draaien"

#, sh-format
msgid "Waiting for $APP_LONG_NAME"
msgstr "Wachten op $APP_LONG_NAME"

#, sh-format
msgid "WARNING: $APP_LONG_NAME may have failed to start."
msgstr "WAARSCHUWING: $APP_LONG_NAME is misschien niet gestart."

msgid "Must be root to perform this action."
msgstr "Root vereist om deze handeling uit te voeren."

#, sh-format
msgid "Starting $APP_LONG_NAME"
msgstr "$APP_LONG_NAME starten"

#, sh-format
msgid "Stopping $APP_LONG_NAME"
msgstr "$APP_LONG_NAME stoppen"

#, sh-format
msgid "$APP_LONG_NAME was not running."
msgstr "$APP_LONG_NAME draaide niet."

#, sh-format
msgid "Unable to stop $APP_LONG_NAME."
msgstr "Kon $APP_LONG_NAME niet stoppen."

#, sh-format
msgid "Waiting for $APP_LONG_NAME to exit"
msgstr "Wachten op $APP_LONG_NAME om af te sluiten"

#, sh-format
msgid "Failed to stop $APP_LONG_NAME."
msgstr "Stoppen van $APP_LONG_NAME mislukt."

#, sh-format
msgid "Stopped $APP_LONG_NAME."
msgstr "$APP_LONG_NAME gestopt."

#, sh-format
msgid "Stopping $APP_LONG_NAME gracefully"
msgstr "$APP_LONG_NAME sierlijk stoppen"

#, sh-format
msgid "$APP_LONG_NAME is not running."
msgstr "$APP_LONG_NAME draait niet."

#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid"
msgstr "$APP_LONG_NAME draait: PID:$pid"

#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid, Wrapper:$STATUS, Java:$JAVASTATUS"
msgstr "$APP_LONG_NAME draait: PID:$pid, Wrapper: $STATUS, Java: $JAVASTATUS"

#, sh-format
msgid "The $APP_LONG_NAME daemon is already installed."
msgstr "De $APP_LONG_NAME daemon is al geïnstalleerd."

#, sh-format
msgid "Installing the $APP_LONG_NAME daemon"
msgstr "Bezig met installeren van de daemon voor $APP_LONG_NAME"

#, sh-format
msgid "Install not currently supported for $DIST_OS"
msgstr "Installeren wordt momenteel niet ondersteund voor $DIST_OS"

#, sh-format
msgid "Removing $APP_LONG_NAME daemon"
msgstr "Bezig met verwijderen van daemon voor $APP_LONG_NAME"

#, sh-format
msgid "The $APP_LONG_NAME daemon is not currently installed."
msgstr "De daemon voor $APP_LONG_NAME is momenteel niet geïnstalleerd."

#, sh-format
msgid "Remove not currently supported for $DIST_OS"
msgstr "Verwijderen wordt momenteel niet ondersteund voor $DIST_OS"

msgid "Commands:"
msgstr "Opdrachten:"

msgid "Launch in the current console."
msgstr "Starten in de huidige console."

msgid "Start in the background as a daemon process."
msgstr "Starten in de achtergrond als een daemonproces."

msgid "Stop if running as a daemon or in another console."
msgstr "Stop indien draaiende als een daemon of in een andere console."

msgid "Stop gracefully, may take up to 11 minutes or more."
msgstr "Stop rustig, dit kan tot 11 minuten duren."

msgid "Restart gracefully, may take up to 11 minutes or more."
msgstr "Herstart netjes. Dit kan tot 11 minuten duren."

msgid "Stop if running and then start."
msgstr "Indien draaiende, stop en start opnieuw."

msgid "Restart only if already running."
msgstr "Herstart enkel indien al draaiende."

msgid "Query the current status."
msgstr "De huidige status opvragen."

msgid "Install to start automatically when system boots."
msgstr "Installeer om automatisch te starten wanneer het systeem opstart."

msgid "Uninstall."
msgstr "Verwijderen."

msgid "Request a Java thread dump if running."
msgstr "Vraag een Java-thread-dump indien draaiende."

msgid "Please edit i2prouter and set the variable RUN_AS_USER"
msgstr "Bewerk i2prouter en stel de parameter RUN_AS_USER in"

msgid "Running I2P as the root user is *not* recommended."
msgstr "I2P uitvoeren als root wordt *niet* aanbevolen."

msgid "To run as root anyway, edit i2prouter and set ALLOW_ROOT=true."
msgstr "Om toch als root uit te voeren, bewerk i2prouter en stel ALLOW_ROOT=true in."
