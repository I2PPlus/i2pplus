msgid "Failed to load the wrapper"
msgstr "Caricamento wrapper non riuscito"

#, sh-format
msgid "$APP_LONG_NAME is already running."
msgstr "$APP_LONG_NAME è già in esecuzione."

#, sh-format
msgid "Running $APP_LONG_NAME"
msgstr "Esecuzione di $APP_LONG_NAME"

#, sh-format
msgid "Waiting for $APP_LONG_NAME"
msgstr "In attesa di $APP_LONG_NAME"

#, sh-format
msgid "WARNING: $APP_LONG_NAME may have failed to start."
msgstr "ATTENZIONE: l'avvio di $APP_LONG_NAME potrebbe non essere riuscito."

msgid "Must be root to perform this action."
msgstr "Occorre essere amministratore per eseguire questa operazione."

#, sh-format
msgid "Starting $APP_LONG_NAME"
msgstr "Avvio di $APP_LONG_NAME"

#, sh-format
msgid "Stopping $APP_LONG_NAME"
msgstr "Interruzione di $APP_LONG_NAME"

#, sh-format
msgid "$APP_LONG_NAME was not running."
msgstr "$APP_LONG_NAME non era in esecuzione."

#, sh-format
msgid "Unable to stop $APP_LONG_NAME."
msgstr "Impossibile interrompere $APP_LONG_NAME."

#, sh-format
msgid "Waiting for $APP_LONG_NAME to exit"
msgstr "In attesa di $APP_LONG_NAME per poter uscire"

#, sh-format
msgid "Failed to stop $APP_LONG_NAME."
msgstr "Interruzione di $APP_LONG_NAME non riuscita."

#, sh-format
msgid "Stopped $APP_LONG_NAME."
msgstr "$APP_LONG_NAME interrotto."

#, sh-format
msgid "Stopping $APP_LONG_NAME gracefully"
msgstr "Interruzione elegante di $APP_LONG_NAME"

#, sh-format
msgid "$APP_LONG_NAME is not running."
msgstr "$APP_LONG_NAME non è in esecuzione."

#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid"
msgstr "$APP_LONG_NAME è in esecuzione: PID:$pid"

#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid, Wrapper:$STATUS, Java:$JAVASTATUS"
msgstr "$APP_LONG_NAME è in esecuzione: PID:$pid, Wrapper:$STATUS, Java:$JAVASTATUS"

#, sh-format
msgid "The $APP_LONG_NAME daemon is already installed."
msgstr "Il demone di $APP_LONG_NAME è già installato."

#, sh-format
msgid "Installing the $APP_LONG_NAME daemon"
msgstr "Installazione del demone di $APP_LONG_NAME"

#, sh-format
msgid "Install not currently supported for $DIST_OS"
msgstr "Installazione non ancora supportata per $DIST_OS"

#, sh-format
msgid "Removing $APP_LONG_NAME daemon"
msgstr "Rimozione del demone di $APP_LONG_NAME"

#, sh-format
msgid "The $APP_LONG_NAME daemon is not currently installed."
msgstr "Il demone di $APP_LONG_NAME non è al momento installato."

#, sh-format
msgid "Remove not currently supported for $DIST_OS"
msgstr "Rimozione non attualmente supportata per $DIST_OS"

msgid "Commands:"
msgstr "Comandi:"

msgid "Launch in the current console."
msgstr "Avvia nel terminale attuale."

msgid "Start in the background as a daemon process."
msgstr "Avvia in background come processo demone."

msgid "Stop if running as a daemon or in another console."
msgstr "Interrompi se è in esecuzione come demone o in un altro terminale."

msgid "Stop gracefully, may take up to 11 minutes or more."
msgstr "Fermati con grazia, potrebbe richiedere fino a 11 minuti."

msgid "Restart gracefully, may take up to 11 minutes or more."
msgstr "Riavviare correttamente, potrebbe richiedere fino a 11 minuti."

msgid "Stop if running and then start."
msgstr "Interrompi se è in esecuzione quindi riavvia."

msgid "Restart only if already running."
msgstr "Riavvia solo se è già in esecuzione."

msgid "Query the current status."
msgstr "Invia query sullo stato attuale."

msgid "Install to start automatically when system boots."
msgstr "Installa per impostare lancio automatico all'avvio del sistema."

msgid "Uninstall."
msgstr "Disinstalla."

msgid "Request a Java thread dump if running."
msgstr "Richiedi un Java thread dump se è in esecuzione."

msgid "Please edit i2prouter and set the variable RUN_AS_USER"
msgstr "Modifica i2prouter e imposta la variabile RUN_AS_USER"

msgid "Running I2P as the root user is *not* recommended."
msgstr "Eseguire I2P come amministratore *non* è consigliato."

msgid "To run as root anyway, edit i2prouter and set ALLOW_ROOT=true."
msgstr "Per eseguirlo comunque come amministratore, modifica  i2prouter e imposta ALLOW_ROOT=true."
