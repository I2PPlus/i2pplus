#: ../i2prouter:236
msgid "Failed to load the wrapper"
msgstr "Käärimen lataus epäonnistui"

#: ../i2prouter:1021 ../i2prouter:1049 ../i2prouter:1123 ../i2prouter:1151
#: ../i2prouter:1172
#, sh-format
msgid "$APP_LONG_NAME is already running."
msgstr "$APP_LONG_NAME on jo käynnissä."

#: ../i2prouter:1032
#, sh-format
msgid "Running $APP_LONG_NAME"
msgstr "Suoritetaan $APP_LONG_NAME"

#: ../i2prouter:1056
#, sh-format
msgid "Waiting for $APP_LONG_NAME"
msgstr "Odotetaan sovellusta $APP_LONG_NAME"

#: ../i2prouter:1103
#, sh-format
msgid "WARNING: $APP_LONG_NAME may have failed to start."
msgstr "VAROITUS: $APP_LONG_NAME käynnistyminen on ehkä epäonnistunut."

#: ../i2prouter:1117 ../i2prouter:1145 ../i2prouter:1351 ../i2prouter:1640
msgid "Must be root to perform this action."
msgstr "Tämän toiminnon suorittamiseksi on oltava root-käyttäjä."

#: ../i2prouter:1161
#, sh-format
msgid "Starting $APP_LONG_NAME"
msgstr "Käynnistetään $APP_LONG_NAME"

#: ../i2prouter:1183
#, sh-format
msgid "Stopping $APP_LONG_NAME"
msgstr "Pysäytetään $APP_LONG_NAME"

#: ../i2prouter:1187 ../i2prouter:1255 ../i2prouter:1808
#, sh-format
msgid "$APP_LONG_NAME was not running."
msgstr "$APP_LONG_NAME ei ole käynnissä."

#: ../i2prouter:1200 ../i2prouter:1208 ../i2prouter:1270 ../i2prouter:1278
#, sh-format
msgid "Unable to stop $APP_LONG_NAME."
msgstr "Sovelluksen $APP_LONG_NAME pysäyttäminen epäonnistui."

#: ../i2prouter:1226
#, sh-format
msgid "Waiting for $APP_LONG_NAME to exit"
msgstr "Odotetaan sovelluksen $APP_LONG_NAME suljeutumista"

#: ../i2prouter:1240
#, sh-format
msgid "Failed to stop $APP_LONG_NAME."
msgstr "Sovelluksen $APP_LONG_NAME pysäyttäminen epäonnistui."

#: ../i2prouter:1243
#, sh-format
msgid "Stopped $APP_LONG_NAME."
msgstr "Sovellus $APP_LONG_NAME pysäytetty."

#: ../i2prouter:1251
#, sh-format
msgid "Stopping $APP_LONG_NAME gracefully"
msgstr "Pysäytetään $APP_LONG_NAME siististi"

#: ../i2prouter:1297
#, sh-format
msgid "$APP_LONG_NAME is not running."
msgstr "$APP_LONG_NAME ei ole käynnissä."

#: ../i2prouter:1302
#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid"
msgstr "$APP_LONG_NAME on käynnissä: PID:$pid"

#: ../i2prouter:1305
#, sh-format
msgid "$APP_LONG_NAME is running: PID:$pid, Wrapper:$STATUS, Java:$JAVASTATUS"
msgstr ""
"$APP_LONG_NAME on käynnissä: PID:$pid, Käärin:$STATUS, Java:$JAVASTATUS"

#: ../i2prouter:1358 ../i2prouter:1370 ../i2prouter:1389 ../i2prouter:1406
#: ../i2prouter:1473 ../i2prouter:1493 ../i2prouter:1507 ../i2prouter:1521
#: ../i2prouter:1549 ../i2prouter:1587 ../i2prouter:1622
#, sh-format
msgid "The $APP_LONG_NAME daemon is already installed."
msgstr "$APP_LONG_NAME-taustaprosessi on jo asennettu."

#: ../i2prouter:1361 ../i2prouter:1376 ../i2prouter:1478 ../i2prouter:1510
#: ../i2prouter:1524 ../i2prouter:1538 ../i2prouter:1552 ../i2prouter:1590
#: ../i2prouter:1625
#, sh-format
msgid "Installing the $APP_LONG_NAME daemon"
msgstr "Asennetaan $APP_LONG_NAME-taustaprosessi"

#: ../i2prouter:1632
#, sh-format
msgid "Install not currently supported for $DIST_OS"
msgstr "Asennus $DIST_OS -järjestelmiin ei tällä hetkellä ole tuettu."

#: ../i2prouter:1648 ../i2prouter:1661 ../i2prouter:1675 ../i2prouter:1684
#: ../i2prouter:1694 ../i2prouter:1718 ../i2prouter:1731 ../i2prouter:1743
#: ../i2prouter:1761 ../i2prouter:1774 ../i2prouter:1788
#, sh-format
msgid "Removing $APP_LONG_NAME daemon"
msgstr "Poistetaan $APP_LONG_NAME-taustaprosessi"

#: ../i2prouter:1654 ../i2prouter:1669 ../i2prouter:1678 ../i2prouter:1688
#: ../i2prouter:1699 ../i2prouter:1712 ../i2prouter:1724 ../i2prouter:1737
#: ../i2prouter:1755 ../i2prouter:1768 ../i2prouter:1782 ../i2prouter:1793
#, sh-format
msgid "The $APP_LONG_NAME daemon is not currently installed."
msgstr "$APP_LONG_NAME-taustaprosessia ei ole asennettu."

#: ../i2prouter:1797
#, sh-format
msgid "Remove not currently supported for $DIST_OS"
msgstr ""
"Poistamista ei tällä hetkellä ole toteutettu  $DIST_OS -järjestelmissä."

#: ../i2prouter:1884
msgid "Commands:"
msgstr "Komennot:"

#: ../i2prouter:1885
msgid "Launch in the current console."
msgstr "Käynnistä nykyisessä pääteikkunassa."

#: ../i2prouter:1886
msgid "Start in the background as a daemon process."
msgstr "Käynnistä taustalla taustaprosessina."

#: ../i2prouter:1887
msgid "Stop if running as a daemon or in another console."
msgstr ""
"Pysäytä, jos jo suoritetaan taustaprosessina tai toisessa pääteikkunassa."

#: ../i2prouter:1888
#, fuzzy
msgid "Stop gracefully, may take up to 11 minutes or more."
msgstr "Pysäytetään siististi, voi kestää jopa 11 minuuttia."

#: ../i2prouter:1889
#, fuzzy
msgid "Restart gracefully, may take up to 11 minutes or more."
msgstr "Pysäytetään siististi, voi kestää jopa 11 minuuttia."

#: ../i2prouter:1890
msgid "Stop if running and then start."
msgstr "Pysäytä, jos käynnissä ja käynnistä sitten."

#: ../i2prouter:1891
msgid "Restart only if already running."
msgstr "Käynnistä uudelleen vain jos on jo käynnissä."

#: ../i2prouter:1892
msgid "Query the current status."
msgstr "Tarkasta tämänhetkinen tila."

#: ../i2prouter:1893
msgid "Install to start automatically when system boots."
msgstr "Asenna käynnistymään automaattisesti järjestelmän käynnistyessä."

#: ../i2prouter:1894
msgid "Uninstall."
msgstr "Poista asennus."

#: ../i2prouter:1895
msgid "Request a Java thread dump if running."
msgstr "Pyydä Java-säievedosta, jos on käynnissä"

#: ../i2prouter:1908
msgid "Please edit i2prouter and set the variable RUN_AS_USER"
msgstr "Muokkaa tiedostoa i2prouter ja aseta muuttuja RUN_AS_USER"

#: ../i2prouter:1913
msgid "Running I2P as the root user is *not* recommended."
msgstr "I2P:n suorittamista root-käyttäjänä *ei* suositella."

#: ../i2prouter:1916
msgid "To run as root anyway, edit i2prouter and set ALLOW_ROOT=true."
msgstr ""
"Jos kuitenkin haluat suorittaa ohjelman root-käyttäjänä, muokkaa tiedostoa "
"i2prouter ja aseta ALLOW_ROOT=true."
