#: ../i2prouter:1884
msgid "Commands:"
msgstr "Kommandoer:"

#: ../i2prouter:1885
msgid "Launch in the current console."
msgstr "Start i gjeldende konsoll."

#: ../i2prouter:1886
msgid "Start in the background as a daemon process."
msgstr "Start i bakgrunnen som en daemon-prosess."

#: ../i2prouter:1887
msgid "Stop if running as a daemon or in another console."
msgstr "Stopp hvis kjører som en daemon eller i en annen konsoll."

#: ../i2prouter:1888
msgid "Stop gracefully, may take up to 11 minutes or more."
msgstr "Stopp på en høflig måte, kan ta opptil 11 minutter eller mer."

#: ../i2prouter:1889
msgid "Restart gracefully, may take up to 11 minutes or more."
msgstr "Start på nytt på en høflig måte, kan ta opptil 11 minutter eller mer."

#: ../i2prouter:1890
msgid "Stop if running and then start."
msgstr "Stopp hvis kjører og start deretter."

#: ../i2prouter:1891
msgid "Restart only if already running."
msgstr "Start på nytt bare hvis det allerede kjører."

#: ../i2prouter:1892
msgid "Query the current status."
msgstr "Sjekk den aktuelle statusen."

#: ../i2prouter:1893
msgid "Install to start automatically when system boots."
msgstr "Installer for å starte automatisk når systemet starter."

#: ../i2prouter:1894
msgid "Uninstall."
msgstr "Avinstaller."

#: ../i2prouter:1895
msgid "Request a Java thread dump if running."
msgstr "Be om en Java-tråddump hvis den kjører."

#: ../i2prouter:1908
msgid "Please edit i2prouter and set the variable RUN_AS_USER"
msgstr "Vennligst rediger i2prouter og sett variabelen RUN_AS_USER"

#: ../i2prouter:1913
msgid "Running I2P as the root user is *not* recommended."
msgstr "Det anbefales *ikke* å kjøre I2P som root-bruker."

#: ../i2prouter:1916
msgid "To run as root anyway, edit i2prouter and set ALLOW_ROOT=true."
msgstr "For å kjøre som root likevel, rediger i2prouter og sett ALLOW_ROOT=true."
