 <shortcut name="Open I2P+ Profile Folder"
          target="explorer"
          commandLine="&quot;%programdata%\i2p&quot;"
          iconFile="%systemroot%\system32\shell32.dll"
          iconIndex="3"
          initialState="normal"
          startMenu="no"
          programGroup="yes"
          desktop="no"
          startup="no"/>
