msgstr "གློག་ཐུང་གི་ནང་དོན་ཕབ་མི་བྱེད་པ།"

msgid "Really delete the marked messages?"
msgstr "མཚོན་བའི་གློག་ཐུང་བསུབས་རྒྱུ་ནམ་།"

msgid "Yes, really delete them!"
msgstr "གཏན་ན་བསུབས་རྒྱུ།"

msgid "Delete Selected"
msgstr "འདེམས་པ་བསུབས་པ།"

msgid "Mark All"
msgstr "ཐམས་ཅད་མཚོན་པ།"

msgid "Clear All"
msgstr "ཐམས་ཅད་བཟོ་བཅོས་བྱེད་པ།"

msgid "Change to Folder"
msgstr "ཞབས་ཞུ་ཁང་དུ་བཟོ་བཅོས་བྱེད་པ།"

msgid "First"
msgstr "ཐོག་མ།"

msgid "Previous"
msgstr "སྔོན་མ།"

msgid "Next"
msgstr "མཐར་མ།"

msgid "Last"
msgstr "མཇུག་མ།"

msgid "Really delete this message?"
msgstr "གློག་ཐུང་འདི་བསུབས་རྒྱུ་ནམ་།"

msgid "Yes, really delete it!"
msgstr "གཏན་ན་བསུབས་རྒྱུ།"

msgid "Reply"
msgstr "རྒྱབ་ལན་བཏོན་པ།"

msgid "Reply All"
msgstr "ཐམས་ཅད་ལ་རྒྱབ་ལན་བཏོན་པ།"

msgid "Forward"
msgstr "མུ་མཐུད་བཏོན་པ།"

msgid "Save As"
msgstr "ཐོག་མའི་ཐེམ་ཚིག་རུ་བུད་པ།"

msgid "Delete"
msgstr "བསུབས་པ།"

msgid "Back to Folder"
msgstr "ཞབས་ཞུ་ཁང་དུ་ཕྱིར་ལོག་པ།"

msgid "Move to Folder"
msgstr "ཞབས་ཞུ་ཁང་དུ་བསྐྱོར་བ།"

msgid "View message in new tab"
msgstr "ཤོག་ངོས་གསར་པར་གློག་ཐུང་མཚོན་པ།"

msgid "Folder Page Size"
msgstr "ཞབས་ཞུ་ཁང་གི་ཤོག་ངོས་ཆེ་ཚོར།"

msgid "Set"
msgstr "བཟོ་བཅོས་བྱེད་པ།"

msgid "Advanced Configuration"
msgstr "བཀོད་སྒྲིག་མཐོ་རིམ་པ།"

msgid "Save Configuration"
msgstr "བཀོད་སྒྲིག་བུད་པ།"

msgid "No response from server"
msgstr "ཞབས་ཞུ་ཁང་ནས་རྒྱབ་ལན་མི་རྙེད།"

msgid "Mail server login failed, wrong username or password."
msgstr "གློག་ཐུང་གི་ཞབས་ཞུ་ཁང་ནས་ཐོག་མའི་འཛུལ་བ་བཟོ་མི་བྱེད། མིང་དང་གསང་ཨང་ནོར་འཁྲུལ།"

msgid "Logout and then login again with the correct username and password."
msgstr "ཐོག་མ་བཤིགས་ནས་མིང་དང་གསང་ཨང་ཡང་དག་གིས་ཐོག་མའི་འཛུལ་བ་བཟོ་བྱེད་རོགས།"

msgid "Login failed"
msgstr "ཐོག་མའི་འཛུལ་བ་བཟོ་མི་བྱེད།"

msgid "Server refused connection"
msgstr "ཞབས་ཞུ་ཁང་ནས་སྦྱོར་བར་བཟང་མི་བྱེད།"

msgid "Mail rejected"
msgstr "གློག་ཐུང་བཟོ་བར་བཟང་མི་བྱེད།"
