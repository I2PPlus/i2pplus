msgid "Start I2P"
msgstr "I2Pའགོ་འཛུགས།"

msgid "I2P is starting!"
msgstr "I2Pའགོ་འཛུགས་བཞིན་པ།"

msgid "Starting"
msgstr "འགོ་འཛུགས་བཞིན་པ།"

msgid "Launch I2P Browser"
msgstr "I2Pདྲ་བོ་འགོ་འཛུགས།"

msgid "Configure I2P System Tray"
msgstr "I2Pམ་ལག་པར་སྒྲིག་འཇུག"

msgid "Disable system tray"
msgstr "མ་ལག་པར་བརྒལ་བ།"

msgid "Restart I2P"
msgstr "I2Pཡང་དག་འགོ་འཛུགས།"

msgid "Stop I2P"
msgstr "I2Pམཚམས་འཇོག"

msgid "Restart I2P Immediately"
msgstr "I2Pཡང་དག་འགོ་འཛུགས་དུ་མ།"

msgid "Stop I2P Immediately"
msgstr "I2Pམཚམས་འཇོག་དུ་མ།"

msgid "Cancel I2P Shutdown"
msgstr "I2Pམཚམས་འཇོག་མེད་པར་བཟོ།"

#, java-format
msgid "Shutdown in {0}"
msgstr "{0}ནང་མཚམས་འཇོག"

msgid "Shutdown imminent"
msgstr "མཚམས་འཇོག་གི་དུས་ཤེས་བཞག"

msgid "Network"
msgstr "དྲོན་རྒྱུ།"

msgid "I2P: Right-click for menu"
msgstr "I2P: བཙོན་མཐིལ་ནན་མེནཊེར་བཟོ་བར་མཐིལ་གཡོན་དུ་མནན་པ།"

msgid "Enable notifications"
msgstr "གནས་བབ་རྒྱུ་བཟོ་བཅོས་བཟོ་བ།"

msgid "Disable notifications"
msgstr "གནས་བབ་རྒྱུ་བཟོ་བཅོས་བརྒལ་བ།"