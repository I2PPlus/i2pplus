msgstr "གནད་སྡུད་སྣོད་ཐོ་དེ་ཡང་དག་འགྲུལ་ལམ་ཅིག་དགོས།"

msgid "Data directory does not exist"
msgstr "གནད་སྡུད་སྣོད་ཐོ་མིན་འདུག"

msgid "Not a directory"
msgstr "སྣོད་ཐོ་མིན་འདུག"

msgid "Unreadable"
msgstr "ལྷག་མ་ཚུགས་པ།"

#, java-format
msgid "Data directory changed to {0}"
