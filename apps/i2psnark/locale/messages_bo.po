#, java-format
msgid "Download speed limit is {0}KB/s"
msgstr "ཕབ་ལེན་མགྱོགས་ཚད་ཀྱི་ཚད་ {0}KB/s ཨིན།"

msgid "Note: You may need to stop and restart your torrents or restart I2PSnark in order to effect this change."
msgstr "དྲན་ཐོ།: བསྒྱུར་བཅོས་འདི་ནུས་པ་ལྡན་བཟོ་ནིའི་དོན་ལུ་ ཁྱོད་ཀྱི་ཊོ་རེནཊི་ཚུ་བཀག་བཏང་སླར་འགོ་བཙུགས་ནི་ཡང་ན་ I2PSnark སླར་འགོ་བཙུགས་དགོས་སྲིད།"

msgid "Enabled +0/1 tunnel hop randomization on Inbound tunnels"
msgstr "ནང་འདྲེན་ཊཱནེལ་ཚུ་གུ་ལུ་ +0/1 ཊཱནེལ་ཧོཔ་ཚུད་མི་སེལ་འཐུ་ལྕོགས་ཅན་བཟོ་ཡི།"

msgid "Disabled tunnel hop randomization on Inbound tunnels"
msgstr "ནང་འདྲེན་ཊཱནེལ་ཚུ་གུ་ལུ་ཊཱནེལ་ཧོཔ་ཚུད་མི་སེལ་འཐུ་ལྕོགས་མེདཔ་བཟོ་ཡི།"

msgid "Enabled +0/1 tunnel hop randomization on Outbound tunnels"
msgstr "ཕྱིར་འདྲེན་ཊཱནེལ་ཚུ་གུ་ལུ་ +0/1 ཊཱནེལ་ཧོཔ་ཚུད་མི་སེལ་འཐུ་ལྕོགས་ཅན་བཟོ་ཡི།"

msgid "Disabled tunnel hop randomization on Outbound tunnels"
msgstr "ཕྱིར་འདྲེན་ཊཱནེལ་ཚུ་གུ་ལུ་ཊཱནེལ་ཧོཔ་ཚུད་མི་སེལ་འཐུ་ལྕོགས་མེདཔ་བཟོ་ཡི།"

#, java-format
msgid "Startup delay changed to {0}"
msgstr "འགོ་བཙུགས་ཕྱིར་འགྱངས་ {0} ལུ་བསྒྱུར་བཅོས་འབད་ཡི།"

#, java-format
msgid "Refresh time changed to {0}"
msgstr "དུས་ཚོད་ཡང་སེལ་བའི་ {0} ལུ་བསྒྱུར་བཅོས་འབད་ཡི།"

msgid "Refresh disabled"
msgstr "ཡང་སེལ་བ་ལྕོགས་མེདཔ་བཟོ་ཡི།"

#, java-format
msgid "Page size changed to {0}"
msgstr "ཤོག་ལྷེ་ཚད་ {0} ལུ་བསྒྱུར་བཅོས་འབད་ཡི།"

msgid "New files will be publicly readable"
msgstr "ཡིག་སྣོད་གསརཔ་ཚུ་མི་མང་གིས་ལྷག་ཆོག་པ་འོང་།"

