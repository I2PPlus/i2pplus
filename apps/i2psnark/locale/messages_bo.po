msgstr "I2CP དང་ཊཱནེལ་བསྒྱུར་བཅོས་ཚུ་ཊོ་རེནཊི་ཆ་མཉམ་བཀག་བཏང་ཚར་བའི་ཤུལ་ལས་ནུས་པ་ལྡན་འོང་།"

#, java-format
msgid "I2CP options changed to: {0}"
msgstr "I2CP གདམ་ཁ་ཚུ་: {0} ལུ་བསྒྱུར་བཅོས་འབད་ཡི།"

msgid "Disconnecting old I2CP destination"
msgstr "I2CP འགྲེམ་ས་རྙིངམ་མཐུད་བཏོག་དོ།"

msgid "Unable to connect with the new settings, reverting to the old I2CP settings"
msgstr "སྒྲིག་སྟངས་གསརཔ་དང་མཐུད་མ་ཚུགས་པས་ I2CP སྒྲིག་སྟངས་རྙིངམ་ལུ་སླར་རྒྱུག་དོ།"

msgid "Unable to reconnect with the old settings!"
msgstr "སྒྲིག་སྟངས་རྙིངམ་ཐོག་ལས་སླར་མཐུད་མ་ཚུགས།"

msgid "Reconnected on the new I2CP destination"
msgstr "I2CP འགྲེམ་ས་གསརཔ་གུ་ལུ་སླར་མཐུད་ཡི།"

#, java-format
msgid "I2CP listener restarted for \"{0}\""
msgstr "\"{0}\" གི་དོན་ལུ་ I2CP གསལ་ཉན་པ་སླར་འགོ་བཙུགས་ཡི།"

msgid "Enabled autostart"
msgstr "རང་བསྐྱོད་འགོ་བཙུགས་ལྕོགས་ཅན་བཟོ་ཡི།"

msgid "Disabled autostart"
msgstr "རང་བསྐྱོད་འགོ་བཙུགས་ལྕོགས་མེདཔ་བཟོ་ཡི།"

msgid "Enabled open trackers - torrent restart required to take effect."
msgstr "ཁ་ཕྱེ་བའི་ཊེ་རེཀར་ཚུ་ལྕོགས་ཅན་བཟོ་ཡི། - ནུས་པ་ལྡན་བཟོ་ནིའི་དོན་ལུ་ཊོ་རེནཊི་སླར་འགོ་བཙུགས་དགོས།"

msgid "Disabled open trackers - torrent restart required to take effect."
msgstr "ཁ་ཕྱེ་བའི་ཊེ་རེཀར་ཚུ་ལྕོགས་མེདཔ་བཟོ་ཡི། - ནུས་པ་ལྡན་བཟོ་ནིའི་དོན་ལུ་ཊོ་རེནཊི་སླར་འགོ་བཙུགས་དགོས།"

msgid "Enabled DHT."
msgstr "DHT ལྕོགས་ཅན་བཟོ་ཡི།"

msgid "Disabled DHT."
msgstr "DHT ལྕོགས་མེདཔ་བཟོ་ཡི།"

msgid "DHT change requires tunnel shutdown and reopen"
msgstr "DHT བསྒྱུར་བཅོས་ལུ་ཊཱནེལ་ཁ་བསྡམས་ནི་དང་སླར་འཛུགས་དགོས།"

msgid "Enabled Ratings."
msgstr "སྐུགས་བརྩེ་དཔྱད་ཚུ་ལྕོགས་ཅན་བཟོ་ཡི།"

