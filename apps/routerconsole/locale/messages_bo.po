msgstr "གླེང་ཐུང་གྲངས་རོགས་སྒྲིག།"

msgid "Bandwidth share percentage (0-100)"
msgstr "གླེང་ཐུང་གྲངས་མཉམ་སྤེལ་གྲངས་ཚད། (༠-༡༠༠)"

msgid "Inbound bandwidth limit (Bps)"
msgstr "ནང་འཛུལ་གླེང་ཐུང་གྲངས་ཚད། (Bps)"

msgid "Outbound bandwidth limit (Bps)"
msgstr "ཕྱིར་འཐེན་གླེང་ཐུང་གྲངས་ཚད། (Bps)"

msgid "SettingsSaved (boolean), RestartNeeded (boolean), plus current values for queried settings"
msgstr "རོགས་སྒྲིག་ཉར་ཚགས་ (boolean), བསྐྱར་འགོད་དགོས་ (boolean), འདྲི་གཞི་བྱས་པའི་རོགས་སྒྲིག་ཀྱི་ད་ལྟའི་རིམ་໖།"

msgid "Manages I2PControl service settings including password, port, and restart configuration."
msgstr "གི་མིང་དང་སྒོ་གྲམ། བསྐྱར་འགོད་རོགས་སྒྲིག་ཚུད་པའི་I2PControl ཞབས་ཞུའི་རོགས་སྒྲིག་དོ་དམ།"

msgid "Listen address (string, e.g., 127.0.0.1)"
msgstr "ཉན་ས་གཞན། (string, དཔེ་ན་ 127.0.0.1)"

msgid "New password (string)"
msgstr "གི་མིང་གསར་པ། (string)"

msgid "Listen port (integer, 1-65535)"
msgstr "ཉན་ས་སྒོ་གྲམ། (integer, ༡-༦༥༥༣༥)"

msgid "SettingsSaved (boolean), RestartNeeded (boolean), current values for requested settings"
msgstr "རོགས་སྒྲིག་ཉར་ཚགས་ (boolean), བསྐྱར་འགོད་དགོས་ (boolean), རེ་ཞིབ་བྱས་པའི་རོགས་སྒྲིག་ཀྱི་ད་ལྟའི་རིམ་໖།"

msgid "Access and modify advanced I2P router configuration settings."
msgstr "མཐོ་རིམ་ I2P རོ་ཌེར་རོགས་སྒྲིག་རིམ་໖་འཛིན་ལེན་དང་བཅོས་སྒྲིག།"

msgid "Key-value pairs of advanced settings"
msgstr "མཐོ་རིམ་རོགས་སྒྲིག་གི་གཙོ་གཞུང་-རིམ་໖་མངགས།"

msgid "Updated settings values"
msgstr "གསར་སྒྱུར་བྱས་པའི་རོགས་སྒྲིག་རིམ་໖།"

msgid "Retrieves statistical rate data from router for monitoring and analysis. Useful for creating custom graphs or monitoring applications."
msgstr "ལྟ་ཞིབ་དང་དབེན་འཇོག་གི་ཆེད་དུ་རོ་ཌེར་ནས་ཚད་གཞིའི་གྲངས་གཞི་འཛིན་ལེན། རང་བཞིན་གྱི་གདེང་ཚོད་དམིགས་གསལ་ལ་མཁོ་འདབ་ཡིན།"

msgid "Statistical metric name (string)"
msgstr "ཚད་གཞིའི་མིང་གཞན། (string)"

msgid "Time period in milliseconds (integer)"
msgstr "དུས་ཚོད་དུས་ཡུན་སྐར་མ་གནས་ཚུལ། (integer)"

msgid "Result (number - average value for specified period)"
msgstr "མཇུག་འབྲས། (གྲངས་ - གསལ་ཁ་བྱས་པའི་དུས་ཡུན་གྱི་ཆ་སྙོམས་རིམ་໖།)"

msgid "Network Status Codes"
msgstr "ཨེམ་གླིང་གནས་སྟངས་ཨང་གྲངས།"

#, java-format
msgid "The {0} field returns an integer value representing the current network connectivity status:"
msgstr "{0} སྟེང་ནས་ད་ལྟའི་ཨེམ་གླིང་མཐུད་སྣེ་གནས་སྟངས་སྟེར་བའི་ integer རིམ་໖་ཕྱིར་ལོག།"

msgid "OK - Full connectivity"
msgstr "གནས་སྟངས་ལེགས་པ། - མཐུད་སྣེ་ཡོངས་སུ་འབྱིན་པ།"

msgid "TESTING - Still establishing connectivity"
msgstr "གདེང་ཚོད་བཞིན་པ། - མཐུད་སྣེ་སྒྲིག་བཞིན་པ།"

msgid "FIREWALLED - Behind restrictive firewall"
msgstr "མེ་འགོག་གི་རྒྱབ། - ཚོད་འཛིན་མེ་འགོག་གི་རྒྱབ་ཏུ།"

msgid "HIDDEN - Router in hidden mode"
msgstr "སྦས་པའི་། - རོ་ཌེར་སྦས་པའི་ཐབས་སྟེགས་ནང་།"

msgid "WARN_FIREWALLED_AND_FAST - Firewalled but fast peer"
msgstr "ཉེན་བརྡ།_མེ་འགོག་དང་མགྱོགས་པའི། - མེ་འགོག་ཡོད་ཀྱང་མགྱོགས་པའི་གྲོགས་པོ།"

msgid "WARN_FIREWALLED_AND_FLOODFILL - Firewalled but floodfill"
msgstr "ཉེན་བརྡ།_མེ་འགོག་དང་floodfill ། - མེ་འགོག་ཡོད་ཀྱང་floodfill །"

msgid "WARN_FIREWALLED_WITH_INBOUND_TCP - Firewalled with inbound TCP"
msgstr "ཉེན་བརྡ།_ནང་འཛུལ་TCPདང་མེ་འགོག། - ནང་འཛུལ་ TCP ཡོད་པ་དང་མེ་འགོག་ཡོད།"

msgid "WARN_FIREWALLED_WITH_UDP_DISABLED - Firewalled with UDP disabled"
msgstr "ཉེན་བརྡ།_མེ་འགོག་དང་UDPམེད་པ། - མེ་འགོག་ཡོད་ཀྱང་ UDP མེད་པ།"

msgid "ERROR_I2CP - I2CP error"
msgstr "ནོར་འཁོར།_I2CP - I2CP ནོར་འཁོར།"

msgid "ERROR_CLOCK_SKEW - System clock skew detected"
msgstr "ནོར་འཁོར།_ཆུ་ཚོད་འཁོར་སྒྲིག - མ་ལག་ཆུ་ཚོད་འཁོར་སྒྲིག་གདེང་ཚོད་བྱས།"

msgid "ERROR_PRIVATE_TCP_ADDRESS - Private TCP address configured"
msgstr "ནོར་འཁོར།_TCPཁོགས་གྲམ་སྒེར་གྱི། - སྒེར་གྱི་ TCP ཁོགས་གྲམ་རོགས་སྒྲིག་བྱས།"

msgid "ERROR_SYMMETRIC_NAT - Symmetric NAT detected"
msgstr "ནོར་འཁོར།_ཁ་སྣོན་གཅིག་གི་NAT - ཁ་སྣོན་གཅིག་གི་ NAT གདེང་ཚོད་བྱས།"

msgid "ERROR_UDP_PORT_IN_USE - UDP port already in use"
msgstr "ནོར་འཁོར།_UDPསྒོ་གྲམ་བེད་སྤྱོད་ནང་། - UDP སྒོ་གྲམ་ད་གཟོད་བེད་སྤྱོད་ནང་།"

msgid "ERROR_NO_ACTIVE_PEERS_CHECK_CONNECTION_AND_FIREWALL - No active peers"
msgstr "ནོར་འཁོར།_གྲོགས་པོ་འགུལ་སྐྱོད་མེད་པ་དང་མཐུད་སྣེ་དང་མེ་འགོག - གྲོགས་པོ་འགུལ་སྐྱོད་མེད།"

msgid "ERROR_UDP_DISABLED_AND_TCP_UNSET - UDP disabled, TCP not configured"
msgstr "ནོར་འཁོར།_UDPམེད་པ་དང་TCPམ་གཞི་བ། - UDP མེད་པ།, TCP རོགས་སྒྲིག་བྱས་མེད།"

msgid "Error Codes"
msgstr "ནོར་འཁོར་ཨང་གྲངས།"

msgid "In addition to standard JSON-RPC 2.0 error codes, I2PControl provides specific error codes:"
msgstr "ཚད་གཞི་ JSON-RPC 2.0 ནོར་འཁོར་ཨང་གྲངས་ལས་གཞན་དུ་ I2PControl གིས་དམིགས་བསལ་ནོར་འཁོར་ཨང་གྲངས་མཁོ་འདབ་བྱེད།"

msgid "JSON parse error"
msgstr "JSON དབྱིབས་འགོད་ནོར་འཁོར།"

msgid "Invalid request"
msgstr "གོ་མིན་རེ་ཞིབ།"

msgid "Method not found"
msgstr "བཀོལ་སྤྱོད་ཚུལ་མ་ཐོབ།"

msgid "Invalid parameters"
msgstr "གོ་མིན་ཁ་སྣོན་ཚུལ།"

msgid "Internal error"
msgstr "ནང་གི་ནོར་འཁོར།"

msgid "Invalid password provided"
msgstr "གོ་མིན་གི་མིང་གི་མཁོ་འདབ་བྱས།"

msgid "No authentication token presented"
msgstr "ངེས་གཏན་ཐབས་ཤེལ་མཁོ་འདབ་བྱས་མེད།"

msgid "Authentication token doesn't exist"
msgstr "ངེས་གཏན་ཐབས་ཤེལ་མེད།"

msgid "The provided authentication token was expired and will be removed"
msgstr "མཁོ་འདབ་བྱས་པའི་ངེས་གཏན་ཐབས་ཤེལ་དུས་བཟུང་སོང་བ་དང་ཕྱིར་འདོན་བྱེད།"

msgid "The version of the I2PControl API used wasn't specified, but is required to be specified"
msgstr "བཀོལ་སྤྱོད་པའི་I2PControl API ཕབ་ཨང་གསལ་ཁ་བྱས་མེད་ཀྱང་གསལ་ཁ་བྱེད་དགོས།"

msgid "The version of the I2PControl API specified is not supported by I2PControl"
msgstr "གསལ་ཁ་བྱས་པའི་I2PControl API ཕབ་ཨང་དེ་ I2PControl གིས་རྒྱབ་སྐྱོར་མ་བྱས།"

msgid "Rate Statistics"
msgstr "གྲངས་གཞི་ཚད་གཞི།"

#, java-format
msgid "For a complete list of available rate statistics, see the {0} documentation in the router console or refer to the {1} online documentation. Common rate statistics include:"
msgstr "ཡོད་པའི་གྲངས་གཞི་ཚད་གཞིའི་ཐོབ་ཐང་ཚང་མ་ལ་མཁོ་འདབ་བྱེད་ཆེད། {0} ཡི་ཡིག་ཆ་རོ་ཌེར་གདམ་གའི་ནང་གི་ཡིག་ཆ་གཟིགས་པའམ་ {1} གི་དྲ་ཐོག་ཡིག་ཆར་གཞིགས་གསལ། སྤྱིར་བཏང་གྲངས་གཞི་ཚད་གཞིའི་ནང་དུ་འདུས།"

msgid "Bandwidth receive rate"
msgstr "གླེང་ཐུང་གྲངས་འཛིན་ལེན་གྲངས་གཞི།"

msgid "Bandwidth send rate"
msgstr "གླེང་ཐུང་གྲངས་སྤོར་གྲངས་གཞི།"

msgid "Active peer count"
msgstr "འགུལ་སྐྱོད་གྲོགས་པོའི་གྲངས།"

msgid "Known peer count"
msgstr "ཤེས་ཟིན་པའི་གྲོགས་པོའི་གྲངས།"

msgid "Tunnel test success time"
msgstr "མཚན་ཁུང་གདེང་ཚོད་ལེགས་གྲངས་དུས་ཚོད།"

msgid "Rate Statistics Documentation"
msgstr "གྲངས་གཞི་ཚད་གཞིའི་ཡིག་ཆ།"

#, java-format
msgid "See the {0} documentation in the router console or refer to the {1} online documentation."
msgstr "{0} ཡི་ཡིག་ཆ་རོ་ཌེར་གདམ་གའི་ནང་གི་ཡིག་ཆ་གཟིགས་པའམ་ {1} གི་དྲ་ཐོག་ཡིག་ཆར་གཞིགས་གསལ།"

msgid "Example Usage"
msgstr "བཀོལ་སྤྱོད་དམིགས་གསལ།"

msgid "Get Router Version and Uptime:"
msgstr "རོ་ཌེར་ཕབ་ཨང་དང་སྐབས་ཡུན་ལོན་པ།"

msgid "Get Current Bandwidth Usage:"
msgstr "ད་ལྟའི་གླེང་ཐུང་གྲངས་བེད་སྤྱོད་ལོན་པ།"

msgid "Change Bandwidth Limits:"
msgstr "གླེང་ཐུང་གྲངས་ཚད་བཅོས་སྒྲིག།"

msgid "Graceful Restart:"
msgstr "གཟབ་གཟབ་གིས་བསྐྱར་འགོད།"

msgid "Security Considerations"
msgstr "བདེ་འཇགས་གཙོ་གཞུང་།"

#, java-format
msgid "Always change the default API password from {0} to a strong, unique password"
msgstr "ཚད་གཞི་ API གི་མིང་དེ་ {0} ནས་ཤུགས་ལྡན་དང་ཁ་སྣོན་པའི་གི་མིང་དུ་བཅོས་སྒྲིག་བྱེད་དགོས།"

msgid "Consider binding I2PControl to localhost only if not accessed remotely"
msgstr "རབ་དབུག་ནས་མི་ཐོབ་ན་ I2PControl དེ་ localhost ལ་མཉམ་སྦྲགས་བྱེད་དགོས་གཞིས་གསལ།"

msgid "Use HTTPS when accessing I2PControl over untrusted networks"
msgstr "ཡིད་ཆེས་མི་ཆོད་པའི་ཨེམ་གླིང་ཐོག་ནས་I2PControl ཐོབ་ཐང་བྱེད་སྐབས་ HTTPS བཀོལ་དགོས།"

msgid "Tokens may expire - implement re-authentication logic in your applications"
msgstr "ཐབས་ཤེལ་དུས་བཟུང་སོང་སྲིད་པས་ ཁྱེད་ཀྱི་མཆན་འཇོག་ནང་ངེས་གཏན་འབེབས་རིམ་໖་ལག་བསྟར་བྱེད་དགོས།"

msgid "Monitor I2PControl access logs for unauthorized usage"
msgstr "མཁོ་འདབ་མེད་པའི་བཀོལ་སྤྱོད་ཆེད་ I2PControl ཐོབ་ཐང་ཟིན་བྱེད་ལྟ་ཞིབ་བྱེད།"

msgid "Be cautious with router management operations - they can affect network connectivity"
msgstr "རོ་ཌེར་དོ་དམ་བཀོལ་སྤྱོད་ཚུལ་ལ་དོ་སྣང་བྱེད་དགོས། - དེ་ཚོས་ཨེམ་གླིང་མཐུད་སྣེར་ཤུགས་རེད་འཇོག་ཐུབ།"

msgid "Integration Examples"
msgstr "མཉམ་སྒྲིལ་དམིགས་གསལ།"

msgid "The I2PControl API can be integrated with:"
msgstr "I2PControl API དེ་འདི་དག་དང་མཉམ་སྒྲིལ་བྱེད་ཆོད།"

msgid "Monitoring systems:"
msgstr "ལྟ་ཞིབ་མ་ལག་།"

msgid "Create custom dashboards for router performance"
msgstr "རོ་ཌེར་ནུས་པ་ཆེད་དམིགས་གསལ་གྱི་གདམ་གའི་མཐོང་གྲལ་བཟོ་བ།"

msgid "Automation scripts:"
msgstr "རང་བཞིན་གྱི་ཡིག་ཆ་།"

msgid "Schedule restarts or configuration changes"
msgstr "བསྐྱར་འགོད་དང་རོགས་སྒྲིག་བཅོས་སྒྲིག་གི་དུས་ཐོག་འཆར་འགོད།"

msgid "Mobile applications:"
msgstr "ཁ་པར་མཆན་འཇོག་།"

msgid "Monitor and manage your router remotely"
msgstr "ཁ་སྣོན་ནས་ཁྱེད་ཀྱི་རོ་ཌེར་ལྟ་ཞིབ་དང་དོ་དམ་བྱེད།"

msgid "Network management tools:"
msgstr "ཨེམ་གླིང་དོ་དམ་ཡི་གེ།"

msgid "Integrate I2P status into broader network monitoring"
msgstr "ཨེམ་གླིང་ལྟ་ཞིབ་ཁྲོད་ I2P གནས་སྟངས་མཉམ་སྒྲིལ།"

msgid "Alerting systems:"
msgstr "ཉེན་བརྡ་མ་ལག་།"

msgid "Send notifications based on router status changes"
msgstr "རོ་ཌེར་གནས་སྟངས་འགོག་སྒྱུར་གྱི་གཞིར་བརྡ་སྤེལ་བྱེད།"

msgid "Licensing Information"
msgstr "རེ་ཁུའི་གོ་སྐབས"

msgid "Legal &amp; Licensing"
msgstr "ཁྲིམས་ཉེན་དང་རེ་ཁུ།"

msgid "Introduction to I2P"
msgstr "I2P ནང་འཇུག་བྱེད་པ།"

msgid "Reseeding Your Router"
msgstr "ཁྱོན་རྒྱུའི་མ་ལག་འདྲེན་བརྐྱབ་པ།"

msgid "Reseeding is the bootstrapping process used to find other routers when you first install I2P, or when your router has too few router references remaining. The default settings will work for most people. Change these only if HTTPS is blocked by a restrictive firewall and reseed has failed."
msgstr "ཁྱོན་རྒྱུའི་མ་ལག་འདྲེན་བརྐྱབ་པ་ནི་ཁྱོན་རྒྱུའི་མ་ལག་གི་གཞི་གྲངས་རེད་པ་དང་། ཁྱོན་རྒྱུའི་མ་ལག་གི་གཞི་གྲངས་ཉུང་མོ་བརྐྱབ་པ་བཅས་བྱེད་པ་རེད། དེའི་སྔོན་བཙུགས་ཀྱི་གཞི་གྲངས་ནི་མི་དམངས་མང་པོར་ལེགས་སོ། ཨེ་ཌི་ཨི་ཨེས་ཨེས་ཨི་སྐོར་གཏོང་མི་ཐུབ་པའི་སྒོར་དང་། ཁྱོན་རྒྱུའི་མ་ལག་འདྲེན་བརྐྱབ་པ་མི་ཐུབ་པའི་དུས་ཙམ་གྱི་ཚུལ་གཞི་གྲངས་བཅོས་དགོས།"

msgid "If reseeding has failed, you should first check your network connection."
msgstr "ཁྱོན་རྒྱུའི་མ་ལག་འདྲེན་བརྐྱབ་པ་མི་ཐུབ་ན། ནང་གི་དྲོང་རྒྱུའི་སྦྱོར་བའི་ཚུལ་ལྟ་དགོས།"

msgid "If a firewall is blocking your connections to reseed hosts, you may have access to a proxy."
msgstr "ཁྱོད་ཀྱིས་reseeding གཞི་ཡིག་དང་འབྲེལ་བའི་སྦྱོར་བ་ཁྱོན་རྒྱུའི་མ་ལག་གིས་བཅད་ན་ཡོད་ན། ཁྱོད་ལ་ཁྱོན་རྒྱུའི་མ་ལག་ལ་ཕེབས་པའི་དབང་ཡོད་པ་རེད།"

msgid "The proxy may be remotely hosted or a service running on your computer (localhost/127.0.0.1)."
msgstr "ཁྱོན་རྒྱུའི་མ་ལག་ནི་རྨོང་གི་གཞི་ཡིག་ནས་བཙུགས་པ་ཡང་ཡོད་པ་དང་། ཁྱོད་ཀྱི་རོབ་སྐྲུད་ནང་འཁོར་བའི་ཞབས་ཞུ་ཞིག་ཀྱང་རེད། (localhost/127.0.0.1)"

msgid "Reseed Configuration page"
msgstr "ཁྱོན་རྒྱུའི་མ་ལག་འདྲེན་བརྐྱབ་པའི་གཞི་གྲངས་དང་བཅས་པའི་དྲ་བ།"

#, java-format
msgid "To use a proxy to reseed, configure the proxy type, hostname, and port on the {0}."
msgstr "བྱེད་སྒོ་སྒྲིག་འགོད་བྱེད་པའི་ཆེད་དུ་བྱེད་སྒོའི་རིགས་དང་། ཨང་མིང་། དང་བོར་གནས་གི་གཞི་གྲངས་{0} ལ་སྒྲིག་འགོད་བྱོས།"

msgid "If you are running the Tor Browser, you may use its Tor instance by configuring I2P to use: <i>Proxy type:</i> <code>SOCKS 5</code> <i>Host:</i> <code>127.0.0.1</code> <i>Port:</i> <code>9150</code>."
msgstr "ཁྱོད་ཀྱིས་ཐོར་བྲོསེར་སྤྱོད་ན་ཐོར་གི་སྦྱོར་ཚུལ་ཐོག་གི་I2P སྤྱོད་པར་བྱེད་ཐུབ། དེ་ཡང་ན་བྱེད་སྒོའི་རིགས་：<code>SOCKS 5</code> ཨང་མིང་：<code>127.0.0.1</code> བོར་གནས་：<code>9150</code>"

msgid "If you are running a standalone Tor instance locally, you may use it by configuring I2P to use: <i>Proxy type:</i> <code>SOCKS 5</code> <i>Host:</i> <code>127.0.0.1</code> <i>Port:</i> <code>9050</code>."
msgstr "ཁྱོད་ཀྱིས་གཞི་ནས་ཐོར་གི་སྒྲིག་འགོད་བྱས་ན་I2P ནང་གི་བྱེད་སྒོའི་རིགས་：<code>SOCKS 5</code> ཨང་མིང་：<code>127.0.0.1</code> བོར་གནས་：<code>9050</code>"

msgid "If you have some peers but need more, you may try the I2P Outproxy option. This will not work for the initial reseed when you have 0 known peers. Selecting this option will use the I2P HTTP outproxy (by default on <code>127.0.0.1:4444</code>) or, if configured, the Orchid Tor plugin."
msgstr "ཁྱོད་ཀྱིས་མིང་དོན་གཞན་གཞིར་བཟུང་ཡོད་ཀྱང་དེ་བས་མང་བ་བྱེད་དགོས་ན་I2P གི་ཕྱི་བྱེད་སྒོའི་རིགས་བེད་སྤྱོད་བྱོས། ཁྱོད་ཀྱིས་མིང་དོན་གཞན་མེད་པའི་སྐབས་སུ་འགོའི་རེ་སིིཌ་ལ་འཇོག་མི་ཐུབ། དེའི་རིགས་བོད་དོ་ན་I2P HTTP ཕྱི་བྱེད་སྒོ་（རྒྱུན་དུ་གཞི་ནས་<code>127.0.0.1:4444</code>）དང་། སྒྲིག་འགོད་བྱས་ན་ཨོར་ཁིཌི་ཐོར་གྱི་བརྒྱུད་སྒྲིག་བེད་སྤྱོད་བྱེད་ཐུབ།"

msgid "Then click the <i>Save changes</i> button to confirm the proxy configuration and then the <i>Reseed now</i> button to initiate the reseed process."
msgstr "མཐོང་བརྒྱུད་སྒྲིག་འགོད་གནས་ཚུལ་གཏན་འཁེལ་བྱེད་དུ་གི་ཡོད་བཅོལ་གཏོང་གི་ཡོད་གཤོག་རྒྱུར་དུ་གཤོག་གི་ཡོད་རེ་སིིཌ་བྱེད་རྒྱུར་དུ་གཤོག་གི་ཡོད་གཤོག་རྒྱུར་དུ་གཤོག་གི་ཡོད།"

#, java-format
msgid "If you know and trust somebody that runs I2P, ask them to send you a reseed file generated using this page in their router console. Then, select the <i>Reseed from file</i> option on the {0} using the file you received."
msgstr "ཁྱོད་ཀྱིས་I2P སྤྱོད་པའི་མི་ཤེས་རུང་བཞིན་དུ་བརྩི་ན་ཁྱོད་ཀྱིས་ཁོང་གི་བྷོ་དེར་གི་ཡིག་ཆ་རེ་སིིཌ་བྱེད་པའི་ཡིག་ཆ་གཏོང་བར་བཞེད་པ་བྱོས། གཤམ་གྱི་གཞི་གྲངས་ནང་གི་ཡིག་ཆ་རེ་སིིཌ་བྱེད་པའི་རིགས་བོད་དོ་གི་ཡོད་{0} ལ་གཤོག་རྒྱུར་དུ་གཤོག་གི་ཡོད།"

#, java-format
msgid "If you know and trust somebody that publishes reseed files, ask them for the URL. Then, click the <i>Reseed from URL</i> button on the {0} to reseed using the URL you received."
msgstr "ཁྱོད་ཀྱིས་རེ་སིིཌ་ཡིག་ཆ་ཕྱོགས་པའི་མི་ཤེས་རུང་བཞིན་དུ་བརྩི་ན་ཁྱོད་ཀྱིས་ཁོང་གི་URL གཏོང་བར་བཞེད་པ་བྱོས། གཤམ་གྱི་གཞི་གྲངས་ནང་གི་URL རེ་སིིཌ་བྱེད་པའི་རིགས་བོད་དོ་གི་ཡོད་{0} ལ་གཤོག་རྒྱུར་དུ་གཤོག་གི་ཡོད།"

msgid "How do I reseed manually?"
msgstr "ངས་རེ་སིིཌ་བྱེད་པ་ཇི་ལྟར་བྱེད་དགོས་པ་ཡིན་ནམ།"

msgid "An I2P router usually only needs to be seeded once, to join the network for the first time. However, if the router is running for an extended period without network connectivity it may also need to be reseeded. Reseeding involves fetching multiple \"RouterInfo\" files (bundled into a signed zip-file) from at least two predefined server URLs picked from a volunteer-run group of clearnet HTTPS servers."
msgstr "I2P གི་བྷོ་དེ་ནི་དྲ་བའི་དྲ་བཞག་གི་སྐབས་སུ་རེ་སིིཌ་བྱེད་པ་གཅིག་ཙམ་གཏོང་བར་དགོས། འོན་ཀྱང་བྷོ་དེ་ནི་དྲ་བ་མེད་པའི་ཚེ་ཡང་རེ་སིིཌ་བྱེད་པ་དགོས་པ་ཡོད། རེ་སིིཌ་བྱེད་པ་ནི་མི་འདྲ་བའི་གླེང་བཞག་གི་དྲ་བའི་གཞི་གྲངས་ནང་གི་གླེང་བཞག་གི་གཞི་གྲངས་གཉིས་ཀྱི་ནང་ནས་གླེང་བཞག་གི་ཡིག་ཆ་མང་པོ་འདོན་པ་ཡིན། དེ་ཡང་ན་གསར་བརྗེ་བའི་དྲ་བའི་གཞི་གྲངས་ཀྱི་ནང་ནས་གླེང་བཞག་གི་ཡིག་ཆ་མང་པོ་འདོན་པ་ཡིན།"

msgid "If the number of known peers drops below 50, I2P will attempt to reseed automatically. If the reseed attempt fails, a notification will appear in the sidebar with an option to manually reseed. A failed reseed attempt can occur if your local firewall limits outbound traffic, you are experiencing network connectivity issues, or if the reseed request is blocked entirely."
msgstr "མིང་དོན་གཞན་གི་གྲངས་ཀ་50 ནས་ཤུབ་པ་ནི་I2P གིས་རེ་སིིཌ་བྱེད་པ་རང་བཞིན་གྱིས་བྱེད་པ་ཡིན། རེ་སིིཌ་བྱེད་པ་མི་ཐུབ་ན་མིང་དོན་གཞན་གི་གྲངས་ཀ་མེད་པའི་ཚེ་ཡང་རེ་སིིཌ་བྱེད་པ་དགོས་པ་ཡོད། རེ་སིིཌ་བྱེད་པ་མི་ཐུབ་པའི་རྒྱུ་མཚན་ནི་ཁྱོད་ཀྱིས་བཞིན་གཞི་ནས་དྲ་བའི་གནས་ཚུལ་བར་ན་གཤིབ་པ་དང་། དྲ་བའི་གནས་ཚུལ་གཤིབ་པ་དང་། རེ་སིིཌ་བྱེད་པའི་བཞེད་དོན་ཡོངས་སུ་བར་ན་གཤིབ་པ་བཅས་ཡིན།"

#, java-format
msgid "In the event that you suspect the request is being blocked, you can configure I2P to use a proxy to reseed. Go to the {0} and configure the proxy type, hostname, and port."
msgstr "ཁྱོད་ཀྱིས་བཞེད་དོན་བར་ན་གཤིབ་པ་ཡིན་པའི་བརྟག་དཔྱད་བྱས་ན་ཁྱོད་ཀྱིས་བྱེད་སྒོ་སྒྲིག་འགོད་བྱེད་པའི་ཆེད་དུ་I2P སྤྱོད་ཐུབ། གཤམ་གྱི་གཞི་གྲངས་ནང་གི་བྱེད་སྒོའི་རིགས་དང་། ཨང་མིང་། དང་བོར་གནས་གི་གཞི་གྲངས་བརྒྱུད་སྒྲིག་བྱོས། {0}"

msgid "If you are stuck behind an ISP firewall or filter, you can use the following manual method (non-automated technical solution) to join the I2P network."
msgstr "ཁྱོད་ཀྱིས་གླེང་བཞག་གི་བྷོ་དེའི་བར་ན་གཤིབ་པའི་གནས་ཚུལ་ན་ཁྱོད་ཀྱིས་རེ་སིིཌ་བྱེད་པའི་བརྒྱུད་རིམ་གཞི་གྲངས་ནང་གི་བརྒྱུད་རིམ་བོད་དོ་གི་ཡོད།"

msgid "Joining the I2P Network using a reseed file"
msgstr "རེ་སིིཌ་ཡིག་ཆ་སྤྱོད་ནས་I2P དྲ་བར་སྣོལ་བ།"

msgid "Please contact a known trustworthy friend who has a running I2P router, and ask them for help with reseeding your I2P router. Request that they send you a reseed file exported from their running I2P router. It is vital that the file is exchanged over a secure channel, e.g. encrypted to avoid external tampering (PGP Sign, Encrypt and Verified with a trusted public key). The file itself is unsigned, so please accept files only from known trusted friends. Never import a reseed file if you cannot verify its source."
msgstr "ཁྱོད་ཀྱིས་གཞི་ནས་I2P གི་བྷོ་དེ་སྤྱོད་པའི་མི་ཤེས་རུང་བཞིན་དུ་བརྩི་ན་ཁྱོད་ཀྱིས་ཁོང་གི་བྷོ་དེར་གི་ཡིག་ཆ་རེ་སིིཌ་བྱེད་པའི་ཡིག་ཆ་གཏོང་བར་བཞེད་པ་བྱོས། དེ་ཡང་ན་གསར་བརྗེ་བའི་དྲ་བའི་གཞི་གྲངས་ཀྱི་ནང་ནས་གླེང་བཞག་གི་ཡིག་ཆ་མང་པོ་འདོན་པ་ཡིན། དེ་ཡང་ན་གསར་བརྗེ་བའི་དྲ་བའི་གཞི་གྲངས་ཀྱི་ནང་ནས་གླེང་བཞག་གི་ཡིག་ཆ་མང་པོ་འདོན་པ་ཡིན།"

msgid "To import the received i2preseed.zip file into your local I2P router:"
msgstr "ཁྱོད་ཀྱིས་ཐོན་པའི་ཡིག་ཆ་i2preseed.zip ཁོང་གི་བྷོ་དེར་ནང་ནུས་པའི་ཆེད་དུ་བརྒྱུད་སྒྲིག་བྱོས།"

#, java-format
msgid "Go to the {0}"
msgstr "གཤམ་གྱི་གཞི་གྲངས་ནང་གི་བརྒྱུད་རིམ་བོད་དོ་གི་ཡོད་{0}"

msgid "Under <i>Select zip or su3 file</i> click <i>Browse&hellip;</i>"
msgstr "<i>zip དང་ su3 ཡིག་ཆ་བོད་དོ</i> ནང་གི་གཤོག་རྒྱུར་དུ་གཤོག་གི་ཡོད་བོད་དོ་གི་ཡོད།"

msgid "Select the i2preseed.zip file"
msgstr "ཡིག་ཆ་i2preseed.zip བོད་དོ་གི་ཡོད།"

msgid "Click the <i>Reseed from File</i> button"
msgstr "<i>ཡིག་ཆ་རེ་སིིཌ་བྱེད་པའི་རིགས</i> གཤོག་རྒྱུར་དུ་གཤོག་གི་ཡོད།"

msgid "Service (Wrapper) log"
msgstr "བྱེད་ནུས་པའི་རིམ་པའི་ཐོག་པ།"

#, java-format
msgid "The reseed status will be indicated on the sidebar. You can also check the {0} for the following message: <code>Reseed got 100 router infos from file with 0 errors</code>"
msgstr "རེ་སིིཌ་བྱེད་པའི་གནས་ཚུལ་མིང་དོན་གཞན་གི་གྲངས་ཀ་ནང་ནུས་པར་གསལ་བར་བྱེད་ཐུབ། ཁྱོད་ཀྱིས་གཤམ་གྱི་གཞི་གྲངས་ནང་གི་བརྒྱུད་རིམ་བོད་དོ་གི་ཡོད་{0} <code>ཡིག་ཆ་ནང་ནས་རེ་སིིཌ་བྱེད་པའི་གནས་ཚུལ་གི་ཡིག་ཆ་མང་པོ་འདོན་པ་ཡིན།</code>"

msgid "Sharing a reseed file"
msgstr "རེ་སིིཌ་ཡིག་ཆ་གཤིང་བཤིག་བྱེད་པ།"

msgid "For trusted friends you can use your local I2P router to give them a jump start:"
msgstr "བརྩི་ན་མི་ཤེས་རུང་བཞིན་དུ་ཁྱོད་ཀྱིས་ཁོང་གི་བྷོ་དེར་གི་ཡིག་ཆ་རེ་སིིཌ་བྱེད་པའི་ཡིག་ཆ་གཏོང་བར་བཞེད་པ་བྱོས།"

msgid "Under <i>Create Reseed File</i> click the <i>Create reseed file</i> button"
msgstr "<i>རེ་སིིཌ་ཡིག་ཆ་བཟོ་བཤིག་བྱེད་པ།</i> ནང་གི་གཤོག་རྒྱུར་དུ་གཤོག་གི་ཡོད་བོད་དོ་གི་ཡོད།"

msgid "Securely send the i2preseed.zip file to your friend"
msgstr "ཡིག་ཆ་i2preseed.zip ཁོང་གི་མི་ཤེས་རུང་བཞིན་དུ་གཏོང་བར་བཞེད་པ་བྱོས།"

msgid "Do not reveal this file to unknown users, as it contains sensitive data (100 RouterInfo files) from your own I2P router which potentially could be used by an adversary to decloak your identity. In order to protect your anonymity: you should wait for a few hours/days before sharing the file with your trusted friend. It is also advisable to use this procedure sparingly (no more than once a week)."
msgstr "ཡིག་ཆ་དེ་མི་ཤེས་མི་དང་བཤིག་མི་བྱོས། དེ་ཡང་ན་ཁྱོད་ཀྱིས་རེ་སིིཌ་བྱེད་པའི་གནས་ཚུལ་གི་ཡིག་ཆ་མང་པོ་འདོན་པ་ཡིན། དེ་ཡང་ན་ཁྱོད་ཀྱིས་རེ་སིིཌ་བྱེད་པའི་གནས་ཚུལ་གི་ཡིག་ཆ་མང་པོ་འདོན་པ་ཡིན། ཁྱོད་ཀྱིས་མིང་དོན་གཞན་གི་གྲངས་ཀ་བཟོ་སྒྲིག་བྱེད་པའི་ཆེད་དུ་གཤིབ་པ་དང་། རེ་སིིཌ་བྱེད་པའི་གནས་ཚུལ་གི་ཡིག་ཆ་མང་པོ་འདོན་པ་ཡིན།"

msgid "General guidelines for manual reseeding of I2P"
msgstr "I2P ལ་གཞི་བརྒྱུད་པའི་ལག་སློགས་པའི་སྤྱོད་ཡོད་པའི་གཞི་བརྒྱུད་པ།"

msgid "Do not publicly publish the reseed file or share these files with a friend of a friend!"
msgstr "སྐུར་བཏོན་གྱི་ཡིག་ཆ་དེ་མི་དམངས་པའི་གནས་གོང་དུ་མི་གཏོང་བ་དང་། འདྲེ་ཤོག་གི་འདྲེ་ཤོག་ལ་ཡང་མི་སྟར་བར་བརྩི་དགོས།"

msgid "This file should be used only for a very limited number of friends (less than 3)."
msgstr "ཡིག་ཆ་འདི་ཉིད་མི་ཤེས་པའི་མི་གྲང་ཚུང་ཉུང་བརྒྱུད་དམིགས་བྱེད་དགོས་པ་དང་། མི་གྲང་3ལ་བརྒལ་མི་རུང་བ་ཡིན།"

msgid "The file will only be valid for a limited time (less than 3 weeks)."
msgstr "ཡིག་ཆ་འདི་ཉིད་དུས་ཚུང་བརྒྱུད་དམིགས་ཀྱི་ནུས་པ་ལྡན་པ་དང་། དུས་ཚོད་3བཟང་ལ་བརྒལ་མི་རུང་བ་ཡིན།"

msgid "If you're experiencing issues with running I2P+, the following information may help identify and resolve the problem:"
msgstr "ཁྱོད་ཀྱིས་I2P+སྐོར་གཏོང་ཚེ་དཀའ་ངལ་དུ་འགྲིག་ན། འོག་གི་གནས་ཚུལ་དེ་དཀའ་ངལ་དེ་རང་གཅོད་པར་དཔྱད་དེ་བགོས་པར་དགོས་པ་ཡིན།"

msgid "Be Patient!"
msgstr "ཞི་བར་བཟོས་རོགས།"

msgid "I2P+ may be slow to integrate into network the first time you run it as it bootstraps into the network and learns of additional peers. The longer your I2P+ router is running, the better it will perform, so try and keep your router on as long as you can, 24/7 if possible! If, after 30 minutes, your <i>Active: [connected/recent]</i> count still has less than 10 peers, or your total number of <i>Integrated</i> peers is less than 5, there are several things you can do to check for problems:"
msgstr "I2P+ དང་པར་དུ་དྲ་བརྒྱུད་ལས་པར་ནང་ network ནང་ལ་བར་དུས་པར་མདོ་པ་འབྲི་ནས་ network ལ་ bootstraps into network དང་ peers ཤེས་དོས་པ་དེ་བསྟོན། ཁྱེད་ I2P+ router འཁོར་ལས་གནས་པར་མདོ་པ་རིང་མད་པ་ network ཀྱི་མཆོ་བ་འདྲ་པ། རྒྱུར་གོ་ network ལ་གཞན་བར་ network ལ་ router འཕྲེལ་རྒྱུད་བ་འདེ་ནས་ network ལ་ peers སྒྲིག་བཞིན་བའི་རོགས། 24/7 ལ་རོགས་པར་མདོ་པ་འདེ་ན། གལ་སྐར་ 30 སྐར་མ་རྗེས་པར་མདོ་པ་རིང་མདོ་པ་རྗེས་པར་མདོ་པ། ཁྱེད་ <i>Active: [connected/recent]</i> count སློག་སྐར་པར་ 10 ལས་ཉུང་བ་པའི་ཡོད་པའི་ network ལ་ <i>Integrated</i> peers ཀྱི་མཆོ་ 5 ལས་ཉུང་བ་པའི་ཡོད་པ་མེད། དེའི་དོན་ལ་བརྒྱུད་པ་ལ་ problems ལ་ check བརྒྱུད་པ་ལེད་བསྟོན།:"

msgid "Check your Configuration &amp; Bandwidth Allocation"
msgstr "ཁྱེད་རྒྱུར་བ་ Configuration &amp; Bandwidth Allocation བལྟོགས་པར་བཞིན་བརྒྱུར་བ།"

msgid "I2P+ functions best when you can accurately reflect the speed of your network connection in the <a href=/config>bandwidth configuration section</a>. By default I2P+ is configured with some fairly conservative values that will not suit many use cases, so please take time to review these settings and correct where necessary. The more bandwidth you allocate, <i>specifically</i> upstream bandwidth, the more you will benefit from the network."
msgstr "ཁྱོད་རང་གི་དྲ་ཐོག་གི་འབྲེལ་མཐུད་ཀྱི་མགྱོགས་ཚད་གནད་དོན་དུ་<a href=/config>བརྒྱུད་སྤེལ་རོགས་སྒྲིག་ཁུལ་</a>ནང་འཇོག་ཐུབ་པར་བྱས་ཚེ། I2P+ དགེ་རྩ་ཆེ་བའི་སྒོ་ནས་ལས་འགོ་ཚུགས་ཐུབ། སྔོན་སྒྲིག་གི་སྒོ་ནས། I2P+ ནི་མཚོན་གཞི་བཟང་པོ་ཞིག་གི་ཐོབ་ཐང་གིས་སྒྲིག་འཇུག་བྱས་ཡོད་པ་ཞིབ་བཤེར་གྱིས་མ་ཚད་དགོས་པ་མང་བའི་སྤྱོད་སྒོའི་གནས་སྐབས་དང་མཐུན་པ་མིན་པས། ཚོད་ལྟ་དང་དགོས་པའི་གནས་སྐབས་ནས་ཡོ་བསྲང་བྱེད་པར་དུས་ཚོད་འདེད་གནང་དགོས། ཁྱོད་རང་གིས་བརྒྱུད་སྤེལ་མཐོ་བ་གཏན་འབེབས་བྱེད་དགོས། དེ་ཐོག་ནས་ཁྱོད་རང་དྲ་ཐོག་ནས་ཕན་ཐོབས་ཆེ་བའི་ཐོབ་ཐང་ལྡན་ཐུབ།"

msgid "Firewalls, Modems &amp; Routers"
msgstr "མེ་འགག་སྒོ་དང་མོ་ཌེམ། རུ་ཊར།"

msgid "Where possible, please ensure I2P+/Java is allowed bi-directional port access from the internet by configuring your modem/router/pc firewall accordingly. If you're behind a prohibitive firewall but have unrestricted outbound access, I2P+ can still function; you can turn off inbound access and rely on <a href=http://i2p-projekt.i2p/udp.html target=_blank rel=noreferrer class=sitelink>SSU IP Address Detection</a> (<a href=https://wikipedia.org/wiki/Hole_punching target=_blank rel=noreferrer class='sitelink external'>firewall hole punching</a>) to connect you to the network, and your network status in the side panel will indicate \"Status: Firewalled\". For optimal performance, please ensure I2P's <a href=/confignet#udpconfig>external port</a> is visible from the internet (see below for more information)."
msgstr "ཐུབ་ཚད་ཀྱི་ནང་དུ། I2P+/Java ཞེས་པ་དེར་དྲ་ཚིག་ནས་སྒོ་གཉིས་ཀྱི་ཐོག་ལ་སྦྱོར་སྒོ་(port) བརྒྱུད་ནས་གནས་སྐབས་ཐོག་དུས་མཚམས་མེད་པར་སླུབ་སྒྲུབ་ཆོག་པའི་རྒྱུ་མཚན་དུ་མོ་ཌེམ། རུ་ཊར། དང་རང་གི་མིག་འཕྲུལ་ལག་དེབ་ཀྱི་མེ་འགག་སྒོ་དེ་དག་གི་སྒྲིག་འཇུག་བྱས་རུང་། གལ་སྲིད་ཁྱོད་རང་མེ་འགག་སྒོ་ཤུགས་རྐྱེན་ཅན་ཞིག་གི་ཐོག་ལ་ཡོད་ཀྱང་ཕྱིར་ལ་སྤྱོད་པའི་སྒོ་མཚམས་མེད་པ་ཡོད་ཚེ། I2P+ ནི་ད་དུང་གནས་སྐབས་ཐོག་བདེ་འཇགས་ཀྱིས་བརྒྱུད་སྐྱོད་བྱེད་ཐུབ་པ་ཡིན། ཁྱོད་རང་གིས་ཕྱིར་ལ་སྤྱོད་པའི་ཁ་སྒོ་(inbound access) འགོག་ནས། <a href=http://i2p-projekt.i2p/udp.html target=_blank rel=noreferrer class=sitelink>SSU IP Address Detection</a> (<a href=https://wikipedia.org/wiki/Hole_punching   target=_blank rel=noreferrer class='sitelink external'>མེ་འགག་སྒོའི་ཁུ་བུ་སྟོང་དུ་ཁེབས་པ།</a>) ལ་བརྟེན་ནས་དྲ་ཚིག་དེ་དང་མཐུད་སྒྲིལ་བྱེད་ཐུབ་པ་ཡིན། དེ་བཞིན་ཁྱོད་རང་གི་བརྒྱབ་ཐུར་ཐོག་ལ་གྲུབ་ཆ་མཐུད་སྒྲིལ་གྱི་གནས་ཚུལ་ནི་\"གནས་ཚུལ། མེ་འགག་སྒོ་གཡོག་བཞིན་པ།\" (Status: Firewalled) ཞེས་པ་བརྡ་སྟོན་བྱེད་ངེས་ཡིན། སྤུས་ཚད་མཐོ་ཤོས་ཀྱི་སྒྲུབ་འགོར་རྒྱུ་ཆེད། I2P གི <a href=/confignet#udpconfig>ཕྱི་རོལ་གྱི་སྦྱོར་སྒོ།</a> (external port) ནི་དྲ་ཚིག་ཕྱི་རོལ་ནས་མཐོང་ཐུབ་པར་ཚོད་ལྟ་བྱོས། (ངེས་པར་དུ་རྒྱུ་མཚན་མང་པོ་འོག་གི་གནས་ཚུལ་གང་ཡིན་པ་ལ་གཟིགས།)"

msgid "Check Your Proxy Settings"
msgstr "ཁྱོད་རང་གི་པྲོ་ཁསི་(proxy) སྒྲིག་འཇུག་ཚོད་ལྟ་བྱོས།"

msgid "If you cannot see any websites at all (not even <a href=http://i2p-projekt.i2p/ target=_blank rel=noreferrer>i2p-projekt.i2p</a>), make sure your browser's proxy is set to access http traffic (<i>not</i> https, <i>not</i> socks) via <code>127.0.0.1 port 4444</code>. If you need some help, there's <a href=https://geti2p.net/en/about/browser-config target=_blank rel=noreferrer class='sitelink external'>a guide</a> to configuring your browser for I2P use."
msgstr "གལ་ཚིག་པས་དྲ་ཚིག་སྒོ་གང་ཡང་མཐོང་མི་ཐུབ་པ་(ཡང་ན་<a href=http://i2p-projekt.i2p/ target=_blank rel=noreferrer>i2p-projekt.i2p</a> ཞེས་པ་དེ་ཡང་མིན་) ན། ཁྱོད་རང་གི་དྲ་ཚིག་རྟོག་པའི་པྲོ་ཁསི་(browser proxy) ནི་<code>127.0.0.1:4444</code> གི་ཐོག་ནས་http གྱི་གནས་སྐབས་(traffic) སླུབ་སྒྲུབ་བྱེད་པར་སྒྲིག་དགོས། (<i>https</i> མིན་པ་དང། <i>socks</i> མིན་པ།)། གལ་ཁྱོད་རང་གིས་རོགས་རམ་གང་ཡང་དགོས་ཚེ། I2P ལ་བརྟེན་ནས་ཁྱོད་རང་གི་དྲ་ཚིག་རྟོག་པ་སྒྲིག་འཇུག་བྱེད་པའི་<a href=https://geti2p.net/en/about/browser-config   target=_blank rel=noreferrer class='sitelink external'>མ་རྒྱུ་ཐུན་མོང་།</a> ཞིག་ཡོད།"

msgid "Check Your Logs"
msgstr "ཁྱོད་རང་གི་ཟིན་ཐོ་(log) ཚོད་ལྟ་བྱོས།"

msgid "<a href=/logs>Logs</a> may help resolve a problem. You may wish to paste excerpts in a <a href=http://i2pforum.i2p/ target=_blank rel=noreferrer>forum</a> for help, or perhaps <a href=http://cake.i2p/pastebin/ target=_blank rel=noreferrer class='sitelink external'>paste</a> it instead and reference the link on IRC for help."
msgstr "<a href=/logs>ཟིན་ཐོ་</a>(Logs) ནི་གནད་དོན་གང་རུང་ཞིག་གསལ་བཤད་བྱེད་པར་རོགས་རམ་གནང་ཐུབ་པ་ཡིན། ཁྱོད་རང་གིས་<a href=http://i2pforum.i2p/ target=_blank rel=noreferrer>སྐད་འཕྲིན་ཁང་།</a>(forum) ནང་དུ་དཔེ་མཚོན་བརྗེ་བཤེར་བྱས་ཏེ་རོགས་རམ་ཞུ་ཐུབ་པ་ཡིན། ཡང་ན་<a href=http://cake.i2p/pastebin/ target=_blank rel=noreferrer class='sitelink external'>བརྗེ་བཤེར་བྱས་ཏེ།</a>(paste) IRC ནང་དུ་དྲ་ཐག་མཚོན་སྟོན་བྱས་ནས་རོགས་རམ་ཞུ་ཐུབ་པ་ཡིན།"

msgid "Verify Java is Up to Date"
msgstr "Java ནི་གསར་སྒྱུར་བྱས་ཡོད་མེད་ཚོད་ལྟ་བྱོས།"

msgid "Ensure your Java is up to date. Version 1.8 or higher is required; version 11 or higher is recommended. Check your Java version at the top of <a href=/info>the router information page</a>."
msgstr "ཁྱོད་རང་གི་Java ནི་གསར་སྒྱུར་བྱས་ཡོད་པར་འགྲོ་བར་གཏུགས། སྒྲིག་རིམ་ 1.8 དང་དེ་ཡན་ཆད་གང་རུང་དགོས་པ་ཡིན། སྒྲིག་རིམ་ 11 དང་དེ་ཡན་ཆད་གང་རུང་བརྗེ་བཤེར་བྱས་ཡོད་ན་དགའ། <a href=/info>རུ་ཊར་གྱི་ཆ་འཕྲིན་ཤོག་ངོས།</a>གི་མགོ་བརྒྱབ་ནས་ཁྱོད་རང་གི་Java སྒྲིག་རིམ་གང་ཡིན་པ་ཚོད་ལྟ་བྱོས།"

msgid "Problems running on Legacy Hardware"
msgstr "སྔོན་གྱི་མིག་འཕྲུལ་ལག་ཆས་དེ་དག་གི་སྒོ་ནས་བརྒྱུད་སྐྱོད་བྱེད་སྐབས་ཀྱི་གནད་དོན།"

msgid "[Linux/Unix/Solaris] If you can't start the router with <code>i2p/i2prouter start</code> try the <code>runplain.sh</code> script in the same directory. Root privileges are never required to run I2P+. If you need to compile the <a href=http://i2p-projekt.i2p/jbigi.html target=_blank rel=noreferrer>jBigi library</a> (which is necessary in rare cases), consult the documentation, visit the forums, or visit <a href=irc://127.0.0.1:6668/saltR class=chatlink>the I2P+ channel</a> or <a href=irc://127.0.0.1:6668/i2p-dev class=chatlink>IRC developer channel</a>."
msgstr "[Linux/Unix/Solaris] གལ་སྲིད་ཁྱོད་རང་གིས་<code>i2p/i2prouter start</code> དང་མཐུད་ནས་རུ་ཊར་མགོ་ཙམ་ཡང་མ་རྒྱུགས་པ་ཡིན་ན། དེ་ནི་དེ་དང་གཅིག་གྱུར་གྱི་ཐོག་གི <code>runplain.sh</code> ལུགས་ཚུལ་སྒྲིག་ཆས་ལ་བརྟག་དཔྱད་བྱོས། I2P+ རྒྱུགས་དུས་སུ་རུ་ཊ་དབང་ཆ་(Root privileges) གང་ཡང་དགོས་མེད། གལ་སྲིད་ཁྱོད་རང་གིས་<a href=http://i2p-projekt.i2p/jbigi.html target=_blank rel=noreferrer>jBigi library</a> (དེ་ནི་མཚམས་སྤོ་བའི་གནས་ཚུལ་གྱི་སྐབས་སུ་དགོས་པ་ཡིན) བཀོལ་སྤྱོད་བྱེད་དགོས་ན། གསལ་བཤད་དེ་དག་ལ་བལྟས་ནས། སྐད་འཕྲིན་ཁང་(forum) དུ་སོང་ངམ་ན་<a href=irc://127.0.0.1:6668/saltR class=chatlink>I2P+ ཆུ་ཚད།</a> ཡང་ན་<a href=irc://127.0.0.1:6668/i2p-dev class=chatlink>IRC བཟོ་སྐྲུན་པའི་ཆུ་ཚད།</a>དུ་སོང་བར་གདོང་འདེབས།"

msgid "Enable Universal Plug and Play (UPnP)"
msgstr "Universal Plug and Play (UPnP) སྒྲུབ་འགོར་ཆོག་པར་བཟོས།"

msgid "Your modem or router may support <a href=https://wikipedia.org/wiki/Universal_Plug_and_Play target=_blank rel=noreferrer>Universal Plug &amp; Play</a> (UPnP), which permits automatic port forwarding. Ensure UPnP support for I2P+ is enabled on the <a href=/confignet>config page</a>, then try to activate UPnP on your modem/router and possibly your computer also. Now try restarting the <a href=/>I2P+ router</a>. If successful, I2P+ should report \"Status: OK\" in the side panel once the I2P+ router completes initial connectivity tests."
msgstr "ཁྱོད་རང་གི་མོ་ཌེམ་དང་རུ་ཊར་ནི་<a href=https://wikipedia.org/wiki/Universal_Plug_and_Play   target=_blank rel=noreferrer>Universal Plug & Play</a> (UPnP) གཉེར་སྤྱོད་བྱེད་ཐུབ་པ་ཡིན། དེ་ནི་སྦྱོར་སྒོ་རྣམ་པར་རྒྱལ་བའི་རྣམ་པ་རང་འགུལ་གྱིས་མཐུན་སྒྲིལ་བྱེད་པའི་ཆེད་དུ་ཡིན། <a href=/confignet>སྒྲིག་འཇུག་གི་ཤོག་ངོས།</a>དེར་I2P+ གི UPnP གཉེར་སྤྱོད་བརྗེ་བཤེར་བྱས་ཡོད་མེད་ཚོད་ལྟ་བྱོས། དེ་རྗེས་ཁྱོད་རང་གི་མོ་ཌེམ་དང་རུ་ཊར་དང་། དམིགས་བསལ་དུ་ཁྱོད་རང་གི་མིག་འཕྲུལ་ལག་ཆས་སུ་ཡང་UPnP འཇུག་སྤེལ་བྱས་རུང། འདི་རྗེས་<a href=/>I2P+ རུ་ཊར།</a> ཚོད་མེད་བསྐྱར་འཁོར་བྱེད་རུང་། གལ་སྲིད་མཁྱེན་ནས་གྲུབ་པ་ཡིན་ན། I2P+ ནི་I2P+ རུ་ཊར་གྱིས་མགོ་འཁོར་བའི་འབྲེལ་ཡོད་ཚོད་ལྟ་མཐའ་སྐྱོང་མཚམས་ཆད་པར། བརྒྱབ་ཐུར་གྱི་ཐོག་ཏུ་\"གནས་ཚུལ། བདེ་འཇགས་(Status: OK)\" ཞེས་པ་བརྡ་སྟོན་བྱེད་ངེས་ཡིན།"

msgid "Port Forwarding"
msgstr "སྦྱོར་སྒོ་རྣམ་པར་རྒྱལ་བ།"

msgid "Open <a href=/confignet#udpconfig>I2P+'s port</a> on your modem, router and/or firewall(s) for better connectivity (ideally both UDP and TCP). More information on how to go about port forwarding can be found at <a href=http://portforward.com/ target=_blank rel=noreferrer class='sitelink external'>portforward.com</a>, in addition to our forums and IRC channels listed below. Note that I2P does not support connecting to the internet via an http or socks proxy [patches welcome!], though you can connect to proxies via I2P itself once connected to the network."
msgstr "མཐུན་སྒྲིལ་བདེ་འཇགས་ཡོང་བར་གྱོད་པའི་ཆེད་དུ། ཁྱོད་རང་གི་མོ་ཌེམ་དང་རུ་ཊར་དང་། ཡང་ན་མེ་འགག་སྒོ་(firewall) དེ་དག་གི་སྒོ་ནས་<a href=/confignet#udpconfig>I2P+ གི་སྦྱོར་སྒོ།</a> (UDP དང་ TCP) གཉིས་ཀ་བརྗེ་བཤེར་བྱོས། སྦྱོར་སྒོ་རྣམ་པར་རྒྱལ་བའི་སྐོར་གྱི་ཆ་འཕྲིན་མང་པོ་ཞིག་<a href=http://portforward.com/ target=_blank rel=noreferrer class='sitelink external'>portforward.com</a>དང་། འོག་གི་སྐད་འཕྲིན་ཁང་དང་IRC ཆུ་ཚད་རྣམས་སུ་མཐོང་ཐུབ་པ་ཡིན། སྐད་པ་གཅིག་མ་ལགས་པ་ནི། I2P ནི་ http དང་ socks བཅས་ཀྱི་པྲོ་ཁསི་(proxy) བརྒྱུད་ནས་དྲ་ཚིག་སྤྱོད་པར་སྒྲུབ་འགོར་མི་བྱེད་པ་ཡིན་[ཁྱོན་ཚུད་བཀོད་སྒྲིག་གི་རོགས་རམ་བསྩལ་བ་ལ་ཤུགས་རྒྱས་བཀུར་བསྟོད་བྱེད།] འོན་ཀྱང་དྲ་ཚིག་དང་མཐུད་སྒྲིལ་མཚམས་ཆད་རྗེས། I2P ལ་བརྟེན་ནས་པྲོ་ཁསི་(proxies) ཐོག་མཐུད་སྒྲིལ་བྱེད་ཐུབ་པ་ཡིན།"

