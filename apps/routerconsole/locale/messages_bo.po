msgstr "xpi2p ཡང་ན་ su3 པ་ལག་རྩལ་ URL ནང་འཇུག་བྱོས།"

msgid "To install a plugin, enter the download URL:"
msgstr "པ་ལག་རྩལ་ཞིག་སྒྲིག་བསྡུད་བྱར། ཕབ་ལེན་ URL ནང་འཇུག་བྱོས།"

msgid "Select xpi2p or su3 file"
msgstr "xpi2p ཡང་ན་ su3 ཡིག་ཆ་གདམ་པ།"

msgid "Please supply a valid plugin file"
msgstr "ནུས་ལྡན་པ་ལག་རྩལ་ཡིག་ཆ་ཞིག་མཁོ་སྤྲོད་བྱོས།"

msgid "Plugin Manager"
msgstr "པ་ལག་རྩལ་འཛིན་སྐྱོང་པ།"

msgid "The plugins listed below are started by the webConsole client."
msgstr "གཤམ་གསལ་གྲལ་བཀོད་པའི་པ་ལག་རྩལ་དེ་དག་ནི་ webConsole ཞབས་འདེགས་ཀྱིས་འགོ་ཚུགས་པ།"

msgid "config reseeding"
msgstr "བཀོད་སྒྲིག་རྒྱུན་དུ་བཙོན་པ།"

msgid "Reseeding is the bootstrapping process used to find other routers when you first install I2P, or when your router has too few router references remaining."
msgstr "རྒྱུན་དུ་བཙོན་པ་ནི་ཁྱེད་ཀྱིས་ཐོག་མར་ I2P སྒྲིག་བསྡུད་བྱེད་སྐབས་ཡང་ན་ཁྱེད་རང་གི་རོ་ཌེར་ལ་རོ་ཌེར་དྲན་ཐོ་ཉུང་དྲགས་པ་ལྷག་སྐབས་གཞན་རོ་ཌེར་འཚོལ་བར་བེད་སྤྱོད་བྱེད་པའི་སྒོ་ནས་འགོ་ཚུགས་པའི་བརྒྱུས་རིམ་ཞིག"

msgid "To enable the configuration of reseed urls, activate the console's <a href=/help/advancedsettings>Advanced mode</a>."
msgstr "བརྒྱུག་པའི་གི་སྲོད་པའི་གི་བརྒྱུག་གི་སྲོད་པའི་གི་བརྒྱུག་པའི་གི་སྲོད་པའི་གི་བརྒྱུག་པའི་གི་མཐར་པའི་གི་ <a href=/help/advancedsettings>སྤྱོས་པའི་གི་བརྒྱུག་པ</a> "

msgid "Reseeding Configuration"
msgstr "རྒྱུན་དུ་བཙོན་པའི་བཀོད་སྒྲིག"

msgid "Help with reseeding"
msgstr "རྒྱུན་དུ་བཙོན་པའི་རོགས་འདེགས།"

msgid "Reseeding Help"
msgstr "རྒྱུན་དུ་བཙོན་པའི་རོགས་འདེགས།"

msgid "Reseed URLs"
msgstr "རྒྱུན་དུ་བཙོན་པའི་ URLs"

msgid "Reseed URL Selection"
msgstr "རྒྱུན་དུ་བཙོན་པའི་ URL གདམ་གསེས།"

msgid "Try SSL first then non-SSL"
msgstr "སྔོན་ལ་ SSL ཚོད་ལྟ་བྱེད་དེ་རྗེས་སུ་ non-SSL"

msgid "Use SSL only"
msgstr "SSL རྐྱང་པོ་བེད་སྤྱོད་བྱོས།"

msgid "Use non-SSL only"
msgstr "non-SSL རྐྱང་པོ་བེད་སྤྱོད་བྱོས།"

msgid "Use Proxy for HTTPS Reseed Hosts"
msgstr "HTTPS རྒྱུན་དུ་བཙོན་པའི་མངའ་དབང་གི་ཚབ་འདེབས་བེད་སྤྱོད་བྱོས།"

msgid "Use Proxy to Reseed"
msgstr "རྒྱུན་དུ་བཙོན་པར་ཚབ་འདེབས་བེད་སྤྱོད་བྱོས།"

msgid "HTTPS"
msgstr "HTTPS"

msgid "SOCKS 4/4a"
msgstr "SOCKS 4/4a"

msgid "SOCKS 5"
msgstr "SOCKS 5"

msgid "This option will use I2P's HTTP outproxy to reseed, or the Orchid plugin if configured. Note: this option will not work when you have 0 known peers."
msgstr "འདི་གདོས་པའི་I2P གི་HTTP ཕྱི་འདྲེན་outproxy དོང་རྒྱུན་སྒྲིག་བྱེད་པ་ཡང་ན་Orchid གློགས་པ་རྒྱུན་སྒྲིག་བྱེད་པ། དྲོན་བརྟག: ཁྱེད་ཀྱི་0 ཤེས་པའི་ལག་གདང་ཚུད་པ་དུས་བཀོད་མི་རྒྱུན་བཞིན་པར་མི་རྒྱུན་བཞིན་པ།"

msgid "I2P Outproxy"
msgstr "I2P Outproxy"

msgid "Port"
msgstr "སྒོ་མོ།"

msgid "Use HTTP Proxy for HTTP Reseed Hosts"
msgstr "HTTP Reseed མངའ་དབང་ལ་དོང་HTTP Proxy བེད་པ།"

msgid "Proxy requires authorization"
msgstr "Proxy དགོས་པར་རེད་བ།"

msgid "Manual Reseed"
msgstr "ལག་ཐོག་པའི་རྒྱུན་སྒྲིག་སྟེན།"

msgid "The su3 format is preferred, as it will be verified as signed by a trusted source."
msgstr "su3 རྒྱུན་སྒྲིག་སྟེན་བསྤེལ་བ། དེ་དོང་གནས་བརྟེན་པའི་གནས་བརྟེན་པར་རེད་བ་པའི་གནས་བརྟེན་པར་རེད་བ།"

msgid "The zip format is unsigned; use a zip file only from a source that you trust."
msgstr "zip རྒྱུན་སྒྲིག་སྟེན་མི་རྒྱུན་སྒྲིག་བྱེད་པ། ཁྱེད་ཀྱི་zip ཡིག་ཆ་ནི་ཁྱེད་ཀྱི་རྒྱུན་སྒྲིག་བྱེད་པའི་ཁྱེད་ཀྱི་བརྟག་པ།"

msgid "Enter zip or su3 URL"
msgstr "zip ཡང་su3 URL བེད་པ།"

msgid "Please supply a valid reseed URL"
msgstr "ནུས་བརྒྱུན་པའི་ནུས་བརྒྱུན་པའི་reseed URL བེད་པར་རོགས་སོ།"

msgid "Select zip or su3 file"
msgstr "zip ཡང་su3 ཡིག་ཆ་བརྒྱུན་པ།"

msgid "Create Reseed File"
msgstr "Reseed ཡིག་ཆ་སྐྱོགས་པ།"

msgid "Create a new reseed zip file you may share for others to reseed manually."
msgstr "གཞིབ་བཞིན་པའི་reseed zip ཡིག་ཆ་བརྒྱུན་པ་སྤེལ་བ་མི་རྒྱུན་བཞིན་པར་གཞིབ་ལ་དོང་ཚུད་པར་ལག་གདང་བཀག་པ།"

msgid "This file will never contain your own router's identity or IP."
msgstr "འདི་ཡིག་ཆ་བརྒྱུན་པ་ནི་ཁྱེད་ཀྱི་རོ་ཌེར་གྱི་ངོས་ཐགས་ཡང་IP མི་ཡིན་པ།"

msgid "Create reseed file"
msgstr "reseed ཡིག་ཆ་སྐྱོགས་པ།"

msgid "config service"
msgstr "བརྒྱུན་སྒྲག་སྟེན།"

msgid "Router Service"
msgstr "རོ་ཌེར་བརྒྱུན་སྒྲག་སྟེན།"

msgid "Shutdown the router"
msgstr "རོ་ཌེར་བརྒྱུན་སྒྲག་བྱེད་པ།"

msgid "Graceful mode allows your router to honor its participating tunnel commitments before shutting down or restarting."
msgstr "བརྒྱུན་སྒྲིག་བྱེད་པས་ཁྱེད་ཀྱི་རོ་ཌེར་གི་གྲུང་ནེལ་ཚུད་པའི་ལག་གདང་ཚུད་པའི་བརྒྱུན་སྒྲིག་བྱེད་པར་མི་རྒྱུན་བཞིན་པར་བསྐྱར་འགོ་ཚུགས་པའམི་བརྒྱུན་སྒྲིག་བྱེད་པར་བསྐྱར་འགོ་ཚུགས་པའམི་བརྒྱུན་སྒྲིག་བྱེད་པ།"

msgid "Restart the router"
msgstr "རོ་ཌེར་བསྐྱར་འགོ་ཚུགས་པ།"

msgid "A restart may be required to update the router, implement a configuration change, or complete installation of a plugin."
msgstr "རོ་ཌེར་བསྐྱར་འགོ་ཚུགས་པ་རྒྱུན་སྒྲག་བྱེད་པ་རོགས་སོ། རྒྱུན་སྒྲག་བྱེད་པ་ལག་བརྒྱུན་སྒྲག་བྱེད་པ་ཡང་བརྒྱུན་སྒྲག་བྱེད་པ་གཞིབ་ལ་དོང་ཚུད་པར་ལག་གདང་བཀག་པ།"

msgid "Launch console at startup"
msgstr "འགོ་ཚུགས་སྐབས་འགོ་ཚུགས་པར་རོགས་སོ།"

msgid "Help with configuring I2P to use a non-system default browser"
msgstr "I2P དོང་བརྒྱུན་སྒྲག་སྟེན་བྱེད་པར་རེད་བཞིན་པར་མིན་མང་མི་རྒྱུན་སྒྲག་སྟེན་བྱེད་པར་རེད་བཞིན་པ།"

msgid "Launch I2P Router Console in web browser when I2P starts."
msgstr "I2P འགོ་ཚུགས་སྐབས་འགོ་ཚུགས་པར་web འགྲ་འདྲེན་ལ་I2P འགོ་ཚུགས་སྐབས་འགོ་ཚུགས་པར་རེད་བ།"

msgid "Debugging"
msgstr "རྐྱེན་སྟོགས་བརྒྱུན་པ།"

#, java-format
msgid "To assist in debugging I2P, it may be helpful to generate a thread dump which will be written to the {0}wrapper log{1}."
msgstr "I2P ཀྱི་ནོར་འཁྲུལ་རྩད་བཤད་ལ་རམ་འདེགས་བྱེད་པའི་ཆེད་དུ། thread dump ཞིག་བསྐྲུན་ནས་ {0}wrapper log{1} ནང་བྲི་བ་ནི་ཕན་པོ་ཡོད།"

msgid "If I2P appears to be using too much ram, you can force the router to run the garbage collection routine to free up memory."
msgstr "I2P དེ་ཀྱི་ram མང་མང་བསྤེལ་བཞིན་པ་ན་རོ་ཌེར་ལ་garbage collection ལས་རིམ་དེ་བཞིན་པར་དྲོག་པའི་དབརྒྱུན་སྒྲག་བྱེད་པ་རེད་བཞིན་པ།"

msgid "System Tray Integration"
msgstr "System Tray མང་མཐའི་བརྒྱུན་སྒྲག་བྱེད་པ།"

msgid "Enable or disable the I2P system tray applet to enable basic service control."
msgstr "I2P system tray མང་ཚན་སྐུ་འགུག་ཡང་མེད་པ་བཟོ་བ་བརྒྱུད་ནས་གཞི་རྩའི་ཞབས་འདེགས་ཚོད་འཛིན་བྱེད།"

msgid "Configure I2P system service"
msgstr "I2P system service རྒྱུན་སྒྲག་སྟེན་བྱེད་པ།"

msgid "Configure the status of the I2P service at system startup. If disabled, the service can be launched manually."
msgstr "I2P ཞབས་འདེགས་ཀྱི་གནས་ཚུལ་རྟེན་གྲངས་འགོ་ཚུགས་སྐབས་བཀོད་སྒྲིག་བྱོས། སྒོ་བརྒྱབ་ཡིན་ན། ཞབས་འདེགས་ལག་ཐབས་བརྒྱུད་ནས་འགོ་ཚུགས་ཐུབ།"

msgid "Install I2P system service"
msgstr "I2P system service སྒྲིག་བསྡུད་བྱོས།"

msgid "Install the I2P system service. This is the recommended way to run I2P."
msgstr "I2P system service སྒྲིག་བསྡུད་བྱོས། འདི་ནི་ I2P གཡོ་བར་འོས་སྦྱོར་བྱེད་པའི་ཐབས་ལམ་ཡིན།"

msgid "config sidebar"
msgstr "བཀོད་སྒྲིག་ཟུར་ངོས་ཕྲ་རིང་།"

msgid "Sidebar Options"
msgstr "ཟུར་ངོས་ཕྲ་རིང་གདམ་གསེས།"

msgid "Refresh interval"
msgstr "གསར་བརྗེས་བར་ནས།"

msgid "seconds"
msgstr "སྐར་ཆ།"

msgid "0 to disable"
msgstr "0 ནི་སྒོ་བརྒྱབ་པ།"

msgid "Unified sidebar"
msgstr "གཅིག་གྱུར་བཟོས་པའི་ཟུར་ངོས་ཕྲེང་རིང་།"

msgid "Use the same sidebar on all pages"
msgstr "ཤོག་ངོས་ཆ་ཚང་ལ་ཟུར་ངོས་ཕྲེང་རིང་གཅིག་པ་བེད་སྤྱོད་བྱོས།"

msgid "Sticky sidebar"
msgstr "གི་པུང"

msgid "Enable conditional fixed sidebar"
msgstr "རང་མཐོད་པའི་གི་གི་པུངང"

msgid "config stats"
msgstr "བཀོད་སྒྲིག་ཚད་རྩིས།"

msgid "Stats Collection &amp; Graphing"
msgstr "ཚད་རྩིས་བསྡུ་ལེན་དང་རིས་འབྲི།"

msgid "Configure I2P Stat Collection"
msgstr "I2P ཚད་རྩིས་བསྡུ་ལེན་བཀོད་སྒྲིག་བྱོས།"

msgid "A limited selection of stats is enabled by default, required for monitoring router performance. Only stats that have an optional graph are listed here; for a full list of enabled stats, view the <a href=\"/stats\">stats page</a>."
msgstr "རོ་ཌེར་གྱི་ནུས་པོ་ལྟ་བཤལ་བྱར་དགོས་པའི་ཚད་རྩིས་ཚད་བཀག་པ་ཞིག་སྔོན་སྒྲིག་བྱས་ཡོད། གདམ་རྐྱང་རིས་འབྲི་ཡོད་པའི་ཚད་རྩིས་རྐྱང་པོ་འདིར་གྲལ་བཀོད་ཡོད། སྐུ་འགུག་བྱས་པའི་ཚད་རྩིས་ཀྱི་ཐོག་ཡོངས་ལ་ལྟ་བར། <a href=\"/stats\">ཚད་རྩིས་ཤོག་ངོས།</a> ལ་གཟིགས།"

msgid "Enable full stats?"
msgstr "ཚད་རྩིས་ཆ་ཚང་སྐུ་འགུག་བྱེད་དགོས་སམ།"

msgid "change requires restart to take effect"
msgstr "བསྒྱུར་བ་དེ་ནུས་པོ་ཐོན་པར་བསྐྱར་འགོ་ཚུགས་དགོས།"

msgid "Select Stats for Graphing"
msgstr "རིས་འབྲི་བྱར་པའི་ཚད་རྩིས་གདམ་པ།"

msgid "config tunnels"
msgstr "བཀོད་སྒྲིག་གསང་ལམ།"

msgid "Tunnel Options"
msgstr "གསང་ལམ་གདམ་གསེས།"

msgid "The default settings work for most people."
msgstr "སྔོན་སྒྲིག་བྱས་པའི་སྒྲིག་བཀོད་དེ་དག་མི་མང་པོར་ལས་འགྲུབ།"

msgid "There is a fundamental tradeoff between anonymity and performance."
msgstr "མིང་མེད་ཐོབ་པ་དང་ནུས་པོ་གི་བར་ནི་གཞི་རྩའི་བརྗེ་བསྒྱུར་ཞིག་ཡོད།"

msgid "Tunnels longer than 3 hops (for example 2 hops + 0-2 hops, 3 hops + 0-1 hops, 3 hops + 0-2 hops), or a high quantity + backup quantity, may severely reduce performance or reliability."
msgstr "གསང་ལམ་ 3 ལས་རིང་བ་(དཔེར་ན། 2 འགྲུལ་བ་ + 0-2 འགྲུལ་བ། 3 འགྲུལ་བ་ + 0-1 འགྲུལ་བ། 3 འགྲུལ་བ་ + 0-2 འགྲུལ་བ་)ཡང་ན་གྲངས་མང་པོ་ + རྒྱབ་གསང་གྲངས་ནི་ནུས་པོ་དང་གཏན་འཁེལ་ལ་གནོད་ཤུགས་བར་གཡོ་སྒྱོགས་བྱེད།"

msgid "High CPU and/or high outbound bandwidth usage may result."
msgstr "CPU མཐོ་བ་དང་/ཡང་ན་ཕྱིན་བཏོན་པའི་རྒྱུན་རིམ་མཐོ་བའི་བེད་སྤྱོད་བྱེད་པ་འབྱུང་སྲིད།"

msgid "Change these settings with care, and adjust them if you have problems."
msgstr "སྒྲིག་བཀོད་འདི་དག་གཡོ་བར་གདོང་རྩལ་བྱེད་པ་དང་། དཀའ་ངལ་ཡོད་ན་དེ་དག་བསྒྱུར་བཅོས་བྱོས།"

msgid "Exploratory tunnel setting changes are stored in the router.config file."
msgstr "འཚོལ་བཤེར་གསང་ལམ་གྱི་སྒྲིག་བཀོད་བསྒྱུར་བ་དག་ router.config ཡིག་ཆར་ཉར་ཚར་ཡོད།"

msgid "Client tunnel changes are temporary and are not saved."
msgstr "ཞབས་འདེགས་གསང་ལམ་བསྒྱུར་བ་ནི་གནས་སྐབས་ཡིན་པ་དང་ཉར་ཚར་བྱེད་མི་སྲིད།"

msgid "The default settings are optimized for general usage and normally don't need to be changed."
msgstr "སྔོན་སྒྲིག་བྱས་པའི་སྒྲིག་བཀོད་དེ་དག་སྤྱི་བཏོན་པའི་བེད་སྤྱོད་ལ་ལེགས་བཅོས་ཡོད་པ་དང་སྤྱིར་བཏོན་པར་བསྒྱུར་བ་དགོས་མི་སྲིད།"

msgid "To modify client tunnel settings, use the <a href=\"/i2ptunnelmgr\">Tunnel Manager</a>, or the <a href=\"/i2psnark/configure\">Configuration page</a> for I2PSnark."
msgstr "ཞབས་འདེགས་གསང་ལམ་གྱི་སྒྲིག་བཀོད་བསྒྱུར་བར། <a href=\"/i2ptunnelmgr\">གསང་ལམ་འཛིན་སྐྱོང་པ།</a> བེད་སྤྱོད་བྱོས་ཡང་ན། I2PSnark དོང་ <a href=\"/i2psnark/configure\">བཀོད་སྒྲིག་ཤོག་ངོས།</a> བེད་སྤྱོད་བྱོས།"

msgid "To enable session-only changes to the client tunnels, activate the console's <a href=\"/help/advancedsettings\">Advanced mode</a>."
msgstr "ཞབས་འདེགས་གསང་ལམ་ལ་ལོ་རེའི་བསྒྱུར་བ་སྐུ་འགུག་བྱར། སྐུ་སྟེགས་ཀྱི་ <a href=\"/help/advancedsettings\">མཐོ་རིམ་རྣམ་པ།</a> སྐུ་འགུག་བྱོས།"

msgid "config UI"
msgstr "རྒྱུད་འཛུགས་ངོས་སྟོན"

msgid "User Interface Configuration"
msgstr "སྤྱོད་མཁན་ངོས་སྟོན་རྒྱུད་འཛུགས"

msgid "Router Console Theme"
msgstr "རོ་ཌེར་རོགས་སྒྱུར་བཞིན་པ།"

msgid "Apply"
msgstr "འཇུག་སྐྱོགས་པ།"

msgid "Theme selection disabled for Internet Explorer, sorry."
msgstr "Internet Explorer དོང་བརྒྱུན་སྒྲག་སྟེན་བྱེད་པར་རེད་བཞིན་པ།"

msgid "A very old version has been detected. It's not recommended to use I2P with unmaintained browsers."
msgstr "ཤིན་གི་པའི་སྤེལ་བཞིན་པར་རེད་བཞིན་པ། དེད་ཀྱི་མིན་མིན་I2P དོང་བརྒྱུན་སྒྲག་སྟེན་བྱེད་པར་རེད་བཞིན་པ།"

msgid "If you're not using IE, it's likely that your browser is pretending to be IE; please configure your browser (or proxy) to use a different User Agent string if you'd like to access the console themes."
msgstr "ཁྱེད་ཀྱིས་ IE བེད་སྤྱོད་བཞིན་མེད་ན། ཁྱེད་རང་གི་ཝེབ་བརྒྱུད་ནི་ IE ཡིན་ཁུལ་བྱེད་པ་ཡིན་འདྲ། ཁྱེད་ཀྱིས་སྐུ་སྟེགས་ཀྱི་བརྗེ་བར་ལ་འཛུལ་བར་འདོད་ན། ཁྱེད་རང་གི་ཝེབ་བརྒྱུད་(ཡང་ན་ཚབ་འདེབས་) ལ་ User Agent ཡིག་ཕྲེང་གཅིག་པ་བཀོད་སྒྲིག་བྱས་ནས་སྐུ་སྟེགས་ཀྱི་བརྗེ་བར་ལ་འཛུལ་བར་འདོད་ན།"

msgid "Router Console Password"
msgstr "རོ་ཌེར་རོགས་སྒྱུར་བཞིན་པ།"

msgid "Router Console Language"
msgstr "རོ་ཌེར་རོགས་སྒྲག་སྟེན།"

msgid "Please contribute to the router console translation project! Contact the developers in #saltR on IRC to help."
msgstr "རོ་ཌེར་རོགས་སྒྱུར་བཞིན་པའི་སྐད་བརྒྱུད་ལས་གཞུགས་སྐུལ་བྱོས། #saltR ནང་ IRC ལ་བཟོ་བསྐྲུན་པ་དང་འབྲེལ་བ་བྱོས།"

msgid "config update"
msgstr "བཀོད་སྒྲིག་གསར་བསྒྱུར།"

msgid "I2P &amp; News Updates"
msgstr "I2P དང་གནས་ཚུད་གསར་བསྒྱུར།"

msgid "News Updates"
msgstr "གནས་ཚུད་གསར་བསྒྱུར།"

msgid "I2P update policy"
msgstr "I2P གསར་བསྒྱུར་སྲིད་པ།"

msgid "Update check"
msgstr "གསར་བསྒྱུར་ཞིབ་བཤེར།"

msgid "Update In Progress"
msgstr "གསར་བསྒྱུར་བཞིན་པ།"

msgid "Update with signed development builds"
msgstr "མིང་རྟགས་བཏང་བའི་གསར་བསྐྱེད་ལས་དང་བསྒྱུར་བ།"

msgid "Update URL"
msgstr "གསར་བསྒྱུར་ URL"

msgid "To update with unsigned I2P+ release builds: <code>http://skank.i2p/i2pupdate.zip</code> or for the latest unsigned development builds: <code>http://skank.i2p/dev/i2pupdate.zip</code>"
msgstr "མིང་མེད་པའི་ I2P+ འགྲེམ་སྐོན་ལས་དང་བསྒྱུར་བར། <code>http://skank.i2p/i2pupdate.zip</code> ཡང་ན་མཐའི་མིང་མེད་པའི་གསར་བསྐྱེད་ལས་དང་བསྒྱུར་བར། <code>http://skank.i2p/dev/i2pupdate.zip</code>"

msgid "Update with unsigned development builds"
msgstr "མིང་མེད་པའི་གསར་བསྐྱེད་ལས་དང་བསྒྱུར་བ།"

msgid "Advanced Options"
msgstr "མཐོ་རིམ་གདམ་གསེས།"

msgid "In order to maintain anonymity, it is recommended to update using the I2P http proxy. If the news or I2P update is hosted on I2P, using the proxy will be necessary."
msgstr "མིང་མེད་ཐོབ་པར་གཡོ་བར་འགུག་པར། I2P http ངོས་འཛིན་བེད་སྤྱོད་བྱེད་ནས་གསར་བསྒྱུར་བར་འོས་སྦྱོར་བྱེད། གལ་སྲོལ་གནས་ཚུད་ཡང་ I2P གསར་བསྒྱུར་ I2P ནང་བཞག་པ་ཡིན་ན། ངོས་འཛིན་བེད་སྤྱོད་བྱེད་པ་ནི་མཁོ་ངེས་ཡིན།"

msgid "Fetch news using I2P http proxy"
msgstr "I2P http ངོས་འཛིན་བེད་སྤྱོད་བྱེད་ནས་གནས་ཚུད་བསྡུད།"

msgid "Update I2P using I2P http proxy"
msgstr "I2P http ངོས་འཛིན་ཧོས་ཊི་བེད་སྤྱོད་བྱེད་ I2P གསར་བསྒྱུར།"

msgid "I2P proxy host"
msgstr "I2P ངོས་འཛིན་ཧོསཊི།"

msgid "I2P proxy port"
msgstr "I2P ངོས་འཛིན་ཚོད་རྟགས།"

msgid "News URL"
msgstr "གནས་ཚུད་ URL"

msgid "If I2P cannot update via BitTorrent, it will try these addresses in the order they are listed"
msgstr "I2P BitTorrent བརྒྱུད་ནས་གསར་བསྒྱུར་མ་ཐུབ་ན། ཁ་བྱང་འདི་དག་གྲལ་བཀོད་པའི་རིམ་པ་བཞིན་པར་ཚོད་ལྟ་བྱེད།"

msgid "Update URLs"
msgstr "གསར་བསྒྱུར་ URLs"

msgid "These keys are used to validate official router updates, signed development builds and the official news feed"
msgstr "ལྡེ་མིག་དེ་དག་གཞི་རྩའི་རོ་ཌེར་གསར་བསྒྱུར་མིང་མེད་པ། མིང་མེད་པའི་གསར་བསྒྱུར་བཏང་བའི་གསར་བསྒྱུར་དང་གཞི་རྩའི་གནས་ཚུད་གསལ་ལམ་བརྒྱུད་པར་བརྟེན་གྲངས་བྱེད།"

msgid "Trusted keys"
msgstr "ཡིད་རྩལ་བྱེད་པའི་ལྡེ་མིག"

msgid "Updates will be dispatched via your package manager."
msgstr "གསར་བསྒྱུར་དེ་དག་ཁྱེད་གི་ཐུམ་སྒྲིག་འཛིན་སྐྱོང་པ་བརྒྱུད་ནས་སྤེལ་བ་འགྲུབ།"

msgid "config webapps"
msgstr "བཀོད་སྒྲིག་ཝེབ་གློག"

msgid "The Java web applications listed below are started by the webConsole client and run in the same JVM as the router. They are usually web applications accessible through the router console. They may be complete applications (e.g. i2psnark), front-ends to another client or application which must be separately enabled (e.g. susidns, i2ptunnel), or have no web interface at all (e.g. addressbook)."
msgstr "གཤམ་གསལ་བཀོད་པའི་ Java ཝེབ་གློག་དེ་དག་ webConsole ཞབས་འདེགས་ཀྱིས་འགོ་ཚུགས་པ་དང་རོ་ཌེར་དང་གཅིག་པའི་ JVM ནང་གཡོ་བ། དེ་དག་སྤྱིར་བཏུག་རོ་ཌེར་རོགས་སྒྱུར་བཞིན་པ་བརྒྱུད་ནས་འཛུལ་ཐུབ་པའི་ཝེབ་གློག་ཡིན། དེ་དག་ཆ་ཚང་གི་གློག་(དཔེར། i2psnark) ཡང་ན་གཞན་ཞབས་འདེགས་ཀྱི་སྟེག་སྐུད་ཡང་ན་གློག་ལ་སོ་སྒྲིག་བྱེད་དགོས་པའི་(དཔེར། susidns, i2ptunnel) ཡང་ན་ཝེབ་ངོས་མཐའི་མེད་པའི་(དཔེར། addressbook) ཡིན་པ་ཡོད།"

