msgstr "router"

msgid "master"
msgstr "meester"

msgid "private"
msgstr "privé"

#, java-format
msgid "Redirecting to {0}"
msgstr "Doorverwijzen naar {0}"

msgid "Redirection in progress"
msgstr "Omleiding wordt uitgevoerd"

#, java-format
msgid "Saved {0} to the {1} addressbook, redirecting now."
msgstr "{0} is in het {1} adresboek opgeslagen, je wordt nu doorgestuurd."

#, java-format
msgid "Failed to save {0} to the {1} addressbook, redirecting now."
msgstr "Kan {0} niet opslaan in het adresboek van {1}. Er wordt nu omgeleid.Opslaan van {0} in het {1} adresboek mislukt, je wordt nu doorgestuurd."

msgid "Click here if you are not redirected automatically."
msgstr "Klik hier als je niet automatisch wordt doorgestuurd."

#, java-format
msgid "Saved the authentication for {0}, redirecting now."
msgstr "Authenticatie voor {0} opgeslagen, nu omleiden."
